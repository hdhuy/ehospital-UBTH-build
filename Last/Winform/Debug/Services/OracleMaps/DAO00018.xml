<sqlMap namespace="eHospital" xmlns="http://ibatis.apache.org/mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <statements>
        <!--Revision:0,10ffbbb3401d3928e01e31889e76dd84-->
        <statement id="DAO00018_GetList" resultClass="DataObject">
            select *
            from TM_NHANVIEN
            where BENHVIEN_ID = '$BENHVIEN_ID$'
            <isPropertyAvailable prepend="and" property="DICTIONARY_CODE">
                CHUCDANH_ID in (select DICTIONARY_ID from LST_DICTIONARY where DICTIONARY_CODE in $DICTIONARY_CODE$)
            </isPropertyAvailable>
        </statement>
        <!--Revision:0,f2d5783e2476c759cf0a6e523da157d4-->
        <statement id="DAO00018_GetNameById" resultClass="DataObject">
            select TENNHANVIEN
            from TM_NHANVIEN
            where
            NHANVIEN_ID = '$NHANVIEN_ID$'
        </statement>
    </statements>
</sqlMap>
