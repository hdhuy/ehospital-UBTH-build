<sqlMap namespace="eHospital" xmlns="http://ibatis.apache.org/mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <statements>        
        <statement id="DAO00321_GetList" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
            select *
            from TM_KHOMAU
            where
            TAMNGUNG = '0'
            and BENHVIEN_ID = '$BENHVIEN_ID$'            
            order by TENKHO
        </statement>       
    </statements>
