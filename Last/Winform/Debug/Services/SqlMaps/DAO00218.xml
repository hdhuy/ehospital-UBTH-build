<!--iBatis Map File-->
<sqlMap namespace="eHospital" xmlns="http://ibatis.apache.org/mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <statements>
        <!--Revision:1,e8aec0a7137f75b4b5ea5b598d290d56-->
        <statement id="DAO00218_GetListProcedure" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
            SELECT name AS NAME
            FROM sys.procedures
            ORDER BY name
        </statement>
        <!--Revision:0,959b0d42a56ba89194231c556f30826f-->
        <statement id="DAO00218_GetListParameter" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
            SELECT SUBSTRING(p.name, 2, LEN(p.name) - 1) AS PARAMETER, p.parameter_id AS Idx, CONVERT(varchar(1),p.is_nullable) as Allow_Null
            FROM sys.procedures sp
            INNER JOIN sys.parameters p ON sp.object_id = p.object_id
            WHERE sp.name = '$value$'
            ORDER BY p.parameter_id
        </statement>
        <!--Revision:2,590e9e1e54810eb416c8fcfefa986b22-->
        <procedure id="DAO00218_GetReportData" parameterMap="parameters" resultClass="DataObject">
            $PROCEDURE_NAME$
        </procedure>
    </statements>
  <parameterMaps>
    <parameterMap id="parameters" class="System.Collections.IDictionary">
    </parameterMap>
  </parameterMaps>
</sqlMap>
