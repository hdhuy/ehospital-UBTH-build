            , LOAIBHYT
            , SOTHE
            , NGAYCAP
            , NGAYHIEULUC
            , NGAYHETHIEULUC
            , TINHTHANH_CAPTHE_ID
            , BENHVIEN_ID
            , BENHVIEN_KCB_ID
            , TREN3NAM
            , TREN5NAM
            , TREN6THANG
