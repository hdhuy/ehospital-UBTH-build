            , TAMNGUNG
            , NGAYTAO
            , NGUOITAO_ID
            , KHUVUCSONG_ID
            ) values (
            #BENHNHAN_ID#
            , #LOAIBHYT#
            , #SOTHE#
            , #NGAYCAP#
            , #NGAYHIEULUC#
            , #NGAYHETHIEULUC#
            , #TINHTHANH_CAPTHE_ID#
            , #BENHVIEN_ID#
            , #BENHVIEN_KCB_ID#
            , #TREN3NAM#
            , #TREN5NAM#
            , #TREN6THANG#
            , #NGAYMIENDONGCHITRA#
            , #TAMNGUNG#
            , #NGAYTAO#
            , #NGUOITAO_ID#
            , #KHUVUCSONG_ID#
            )
            else
            update TT_BENHNHAN_BHYT set
            LOAIBHYT = #LOAIBHYT#
            , SOTHE = #SOTHE#
            , NGAYCAP = #NGAYCAP#
            , NGAYHIEULUC = #NGAYHIEULUC#
            , NGAYHETHIEULUC = #NGAYHETHIEULUC#
            , TINHTHANH_CAPTHE_ID = #TINHTHANH_CAPTHE_ID#
