        <statement id="DAO00030_DUOC_TONKHO_BOOKING_Release_All" parameterClass="System.String" resultClass="System.Int32">
          declare @json nvarchar(max);
          set @json = N'{
          "DATA": $JSON_DATA$
          }';

          declare @REF_TABLE nvarchar(100);
          declare @REF_ID int;
          declare @NGUOICAPNHAT_ID int;

          declare @iTotal int = #TOTAL_RECORD#;
          declare @icnt int = 0;

          WHILE @iTotal > @icnt
          BEGIN

          declare @dyn_sql nvarchar(1000) = 'select @data = json_query(@json_dyn, ''' + CHAR(36) +'.DATA[' + cast(@icnt as varchar(10)) + ']'');'
          declare @jsondatamax nvarchar(max);

          exec sp_executesql @dyn_sql
          , N'@json_dyn nvarchar(max), @data nvarchar(max) OUTPUT'
          , @json_dyn = @json
          , @data = @jsondatamax OUTPUT

          SELECT
          @REF_TABLE = JSON_VALUE(@jsondatamax, '' + CHAR(36) +'.REF_TABLE'),
          @REF_ID = JSON_VALUE(@jsondatamax, '' + CHAR(36) +'.REF_ID'),
          @NGUOICAPNHAT_ID = JSON_VALUE(@jsondatamax, '' + CHAR(36) +'.NGUOICAPNHAT_ID')

          IF(@REF_TABLE = 'TT_NOITRU_TOATHUOC' or @REF_TABLE = 'TT_DUOC_PHIEULINH')

          BEGIN
          update TT_DUOC_TONKHO_BOOK_NOITRU set
          SOLUONG = 0
          ,NGAYCAPNHAT = getdate()
          ,NGUOICAPNHAT_ID = @NGUOICAPNHAT_ID
          where
          REF_TABLE = @REF_TABLE
          AND REF_ID = @REF_ID
          END

          ELSE

          BEGIN
          update TT_DUOC_TONKHO_BOOK_NGOAITRU set
          SOLUONG = 0
          ,NGAYCAPNHAT = getdate()
          ,NGUOICAPNHAT_ID = @NGUOICAPNHAT_ID
          where
          REF_TABLE = @REF_TABLE
          AND REF_ID = @REF_ID
          END

          set @icnt = @icnt + 1;

          END
        </statement>
        
