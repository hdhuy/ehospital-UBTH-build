        <statement id="DAO00030_PHIEULINH_SEARCH" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
            select *
            from TT_DUOC_PHIEULINH
            <dynamic prepend="where">
                <isParameterPresent>
                  <isNotEmpty prepend="and" property="CHUNGTU_ID">
