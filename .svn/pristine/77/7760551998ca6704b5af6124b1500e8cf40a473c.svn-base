        <statement id="DAO00030_Search_PhieuLinhDaNhapKho" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
            select
            PL.CHUNGTU_ID,
            PL.MACHUNGTU,
            PL.NGAYCHUNGTU,
