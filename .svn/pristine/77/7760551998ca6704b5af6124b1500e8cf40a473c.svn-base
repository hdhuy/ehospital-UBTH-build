        <statement id="DAO00030_DUOCPHIEULINHCT_AddMulti" parameterClass="System.String" resultClass="System.Int32">
          declare @json nvarchar(max);
          set @json = N'{
          "DATA": $JSON_DATA$
          }';

          declare @CHUNGTUCHITIET_ID int;
          declare @CHUNGTU_ID int;
          declare @DUOC_ID int;
          declare @DONVITINH_ID int;
          declare @NGUONHANG_ID int;
          declare @SOLUONGYEUCAU numeric(25, 4);
          declare @SOLUONGTONGHOPTHUCTE numeric(25, 4);
          declare @REF_TABLE nvarchar(100);
          declare @TOATHUOCNOITRU_ID int;
          declare @TENBENHNHAN nvarchar(100);
          declare @BENHNHAN nvarchar(MAX);
          declare @YLENH nvarchar(MAX);
          declare @MASTER_ID int;
          declare @BENHVIEN_ID varchar(10);
          declare @NGAYTAO datetime;
          declare @NGUOITAO_ID int;
          declare @NGAYCAPNHAT datetime;
          declare @NGUOICAPNHAT_ID int;

          declare @iTotal int = #TOTAL_RECORD#;
          declare @icnt int = 0;

          WHILE @iTotal > @icnt
          BEGIN

          declare @dyn_sql nvarchar(1000) = 'select @data = json_query(@json_dyn, ''' + CHAR(36) +'.DATA[' + cast(@icnt as varchar(10)) + ']'');'
          declare @jsondatamax nvarchar(max);

          exec sp_executesql @dyn_sql
          , N'@json_dyn nvarchar(max), @data nvarchar(max) OUTPUT'
          , @json_dyn = @json
          , @data = @jsondatamax OUTPUT

          SELECT
          @CHUNGTU_ID = JSON_VALUE(@jsondatamax, '' + CHAR(36) +'.CHUNGTU_ID'),
          @DUOC_ID = JSON_VALUE(@jsondatamax, '' + CHAR(36) +'.DUOC_ID'),
          @DONVITINH_ID = JSON_VALUE(@jsondatamax, '' + CHAR(36) +'.DONVITINH_ID'),
          @NGUONHANG_ID = JSON_VALUE(@jsondatamax, '' + CHAR(36) +'.NGUONHANG_ID'),
          @SOLUONGYEUCAU = JSON_VALUE(@jsondatamax, '' + CHAR(36) +'.SOLUONGYEUCAU'),
          @SOLUONGTONGHOPTHUCTE = JSON_VALUE(@jsondatamax, '' + CHAR(36) +'.SOLUONGTONGHOPTHUCTE'),
          @REF_TABLE = JSON_VALUE(@jsondatamax, '' + CHAR(36) +'.REF_TABLE'),
          @TOATHUOCNOITRU_ID = JSON_VALUE(@jsondatamax, '' + CHAR(36) +'.TOATHUOCNOITRU_ID'),
          @TENBENHNHAN = JSON_VALUE(@jsondatamax, '' + CHAR(36) +'.TENBENHNHAN'),
          @BENHNHAN = JSON_VALUE(@jsondatamax, '' + CHAR(36) +'.BENHNHAN'),

          @YLENH = JSON_VALUE(@jsondatamax, '' + CHAR(36) +'.YLENH'),
          @MASTER_ID = JSON_VALUE(@jsondatamax, '' + CHAR(36) +'.MASTER_ID'),
          @BENHVIEN_ID = JSON_VALUE(@jsondatamax, '' + CHAR(36) +'.BENHVIEN_ID'),
          @NGUOITAO_ID = JSON_VALUE(@jsondatamax, '' + CHAR(36) +'.NGUOITAO_ID'),
          @NGUOICAPNHAT_ID = JSON_VALUE(@jsondatamax, '' + CHAR(36) +'.NGUOITAO_ID')

          insert into TT_DUOC_PHIEULINH_CHITIET (
          CHUNGTU_ID
          , DUOC_ID
          , DONVITINH_ID
          , NGUONHANG_ID
          , SOLUONGYEUCAU
          , SOLUONGTONGHOPTHUCTE
          , REF_TABLE
          , TOATHUOCNOITRU_ID
          , TENBENHNHAN
          , BENHNHAN
          , YLENH
          , MASTER_ID
          , DAPHATSINHPHIEUXUAT
          , DAPHATSINHPHIEUHOANTRA
          , MIENPHI
          , BENHVIEN_ID
          , NGAYTAO
          , NGUOITAO_ID
          ) values (
          @CHUNGTU_ID
          , @DUOC_ID
          , @DONVITINH_ID
          , @NGUONHANG_ID
          , @SOLUONGYEUCAU
          , @SOLUONGTONGHOPTHUCTE
          , @REF_TABLE
          , @TOATHUOCNOITRU_ID
          , @TENBENHNHAN
          , @BENHNHAN
          , @YLENH
          , @MASTER_ID
          , '0'
          , '0'
          , '0'
          , @BENHVIEN_ID
          , getdate()
          , @NGUOITAO_ID
          )

          if (@REF_TABLE = 'TT_NOITRU_TOATHUOC')
          BEGIN
          UPDATE  TT_NOITRU_TOATHUOC set PHIEULINH_ID = #PHIEULINH_ID#, NGAYCAPNHAT = getdate(), NGUOICAPNHAT_ID = @NGUOICAPNHAT_ID WHERE TOATHUOC_ID = @TOATHUOCNOITRU_ID
          END

          if (@REF_TABLE = 'TT_PHAUTHUAT_VTYT')
          BEGIN
          UPDATE  TT_PHAUTHUAT_VTYT set CHUNGTUTONGHOP_ID = #PHIEULINH_ID#, NGAYCAPNHAT = getdate(), NGUOICAPNHAT_ID = @NGUOICAPNHAT_ID WHERE PHAUTHUAT_VTYT_ID = @TOATHUOCNOITRU_ID
          END

          if (@REF_TABLE = 'TT_CLSKETQUA_VTYT')
          BEGIN
          UPDATE  TT_CLSKETQUA_VTYT set CHUNGTUTONGHOP_ID = #PHIEULINH_ID#, NGAYCAPNHAT = getdate(), NGUOICAPNHAT_ID = @NGUOICAPNHAT_ID WHERE CLSKETQUA_VTYT_ID = @TOATHUOCNOITRU_ID
          END

          if (@REF_TABLE = 'TT_NGOAITRU_KHAMBENH_VTYT')
          BEGIN
          UPDATE  TT_NGOAITRU_KHAMBENH_VTYT set PHIEULINH_ID = #PHIEULINH_ID#, NGAYCAPNHAT = getdate(), NGUOICAPNHAT_ID = @NGUOICAPNHAT_ID WHERE KHAMBENH_VTYT_ID = @TOATHUOCNOITRU_ID
          END

          set @icnt = @icnt + 1;

          END
        </statement>
