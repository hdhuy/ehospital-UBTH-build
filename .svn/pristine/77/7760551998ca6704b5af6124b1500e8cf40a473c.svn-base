        <statement id="DAO00030_TT_DUOC_TONKHO_KiemTraKiemKe" parameterClass="System.Collections.IDictionary" resultClass="DataObject">      
          SELECT TK.KHODUOC_ID, TK.DUOC_ID, TK.SOLONHAP_ID, TD.TONDAU
          FROM
          (
          <!--/*Tồn kho*/-->
          SELECT
          tk.KHODUOC_ID,
          tk.DUOC_ID,
          TK.SOLONHAP_ID
          FROM
          TT_DUOC_TONKHO tk
          inner join TM_DUOC d on d.DUOC_ID = tk.DUOC_ID
          inner join TT_DUOC_CHUNGTU_SOLONHAP sln on sln.SOLONHAP_ID = tk.SOLONHAP_ID
          WHERE
          CONVERT(date, TK.NGAYCAPNHAT) BETWEEN   CONVERT(date, '$TUNGAY$')  AND   CONVERT(date, '$DENNGAY$')
          AND tk.KHODUOC_ID = '$KHODUOC_ID$'
          AND tk.BENHVIEN_ID = '$BENHVIEN_ID$'
          )TK
          LEFT JOIN
          (
          <!--/*Tồn đầu kỳ của tháng @TUNGAY*/-->
          SELECT
          tdk.KHODUOC_ID,
          tdk.DUOC_ID,
          sln.SOLONHAP_ID,
          sln.SOLONHAP,
          SUM(tdk.SOLUONG) as TONDAU
          FROM
          TT_DUOC_TONDAUKY tdk
          inner join TT_DUOC_KYTONKHO ktk on tdk.DUOCKYTONKHO_ID = ktk.DUOCKYTONKHO_ID
          inner join TT_DUOC_CHUNGTU_SOLONHAP sln on sln.SOLONHAP_ID = tdk.SOLONHAP_ID
          WHERE
          ktk.THANG  = MONTH('$TUNGAY$') AND ktk.NAM  = YEAR('$DENNGAY$')
          AND tdk.KHODUOC_ID = '$KHODUOC_ID$'
          AND tdk.BENHVIEN_ID = '$BENHVIEN_ID$'
          GROUP BY tdk.KHODUOC_ID, tdk.DUOC_ID, sln.SOLONHAP_ID, sln.SOLONHAP, ktk.TUNGAY
          )TD on TK.KHODUOC_ID = TD.KHODUOC_ID AND TK.DUOC_ID = TD.DUOC_ID AND TK.SOLONHAP_ID = TD.SOLONHAP_ID
          WHERE TONDAU IS NOT NULL
        </statement>
      <statement id="DAO00030_TT_DUOC_TONKHO_GetListKiemKe" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
        SELECT TK.KHODUOC_ID, TK.DUOC_ID, TK.TENDUOCDAYDU, TK.SOLONHAP_ID, TK.SOLONHAP, TK.SOLOSANPHAM, TK.HANDUNG as HANSUDUNG, TK.DONVITINH_ID,TK.DONVITINH, TK.DONGIAMUA,
        TK.TONKHO, ISNULL(TD.TONDAU, 0) AS TONDAU, ISNULL(NHAP.SL_NHAP, 0) AS NHAP, ISNULL(XUAT.SL_XUAT, 0) AS XUAT,
        ISNULL(TD.TONDAU, 0) + ISNULL(NHAP.SL_NHAP, 0) - ISNULL(XUAT.SL_XUAT, 0) as SOLUONG
        FROM
        (
        <!--/*Tồn kho*/-->
        SELECT
        tk.KHODUOC_ID,
        tk.DUOC_ID,
        d.TENDUOCDAYDU,
        sln.HANDUNG,
        d.DONVITINH_ID,
        d.DONVITINH,
        sln.SOLONHAP_ID,
        sln.SOLONHAP,
        sln.SOLOSANPHAM,
        sln.DONGIAMUA,
        SUM(tk.SOLUONG) as TONKHO
        FROM
        TT_DUOC_TONKHO tk
        inner join TM_DUOC d on d.DUOC_ID = tk.DUOC_ID
        inner join TT_DUOC_CHUNGTU_SOLONHAP sln on sln.SOLONHAP_ID = tk.SOLONHAP_ID
        WHERE
        tk.KHODUOC_ID = '$KHODUOC_ID$'
        AND tk.BENHVIEN_ID = '$BENHVIEN_ID$'
        GROUP BY tk.KHODUOC_ID, tk.DUOC_ID, d.TENDUOCDAYDU, sln.HANDUNG, d.DONVITINH_ID,d.DONVITINH, sln.SOLONHAP_ID, sln.SOLONHAP, sln.SOLOSANPHAM, sln.DONGIAMUA
        )TK
        LEFT JOIN
        (
        <!--/*Tồn đầu kỳ của tháng @TUNGAY*/-->
        SELECT
        tdk.KHODUOC_ID,
        tdk.DUOC_ID,
        sln.SOLONHAP_ID,
        sln.SOLONHAP,
        SUM(tdk.SOLUONG) as TONDAU
        FROM
        TT_DUOC_TONDAUKY tdk
        inner join TT_DUOC_KYTONKHO ktk on tdk.DUOCKYTONKHO_ID = ktk.DUOCKYTONKHO_ID
        inner join TT_DUOC_CHUNGTU_SOLONHAP sln on sln.SOLONHAP_ID = tdk.SOLONHAP_ID
        WHERE
        ktk.THANG  = MONTH('$TUNGAY$') AND ktk.NAM  = YEAR('$DENNGAY$')
        AND tdk.KHODUOC_ID = '$KHODUOC_ID$'
        AND tdk.BENHVIEN_ID = '$BENHVIEN_ID$'
        GROUP BY tdk.KHODUOC_ID, tdk.DUOC_ID, sln.SOLONHAP_ID, sln.SOLONHAP, ktk.TUNGAY
        )TD on TK.KHODUOC_ID = TD.KHODUOC_ID AND TK.DUOC_ID = TD.DUOC_ID AND TK.SOLONHAP_ID = TD.SOLONHAP_ID

        LEFT JOIN
        (
        <!--/*nhập trong kỳ*/-->
        SELECT
        ct.KHONHAP_ID as KHODUOC_ID, ctct.DUOC_ID, sln.SOLONHAP_ID, sln.SOLONHAP, sum(ctct.SOLUONGTHUCTE) as SL_NHAP
        FROM

        TT_DUOC_CHUNGTU ct
        inner join TT_DUOC_CHUNGTU_CHITIET ctct on ct.CHUNGTU_ID = ctct.CHUNGTU_ID
        inner join TT_DUOC_CHUNGTU_SOLONHAP sln on sln.SOLONHAP_ID = ctct.SOLONHAP_ID
        WHERE
        ct.LOAICHUNGTU = 'N'
        AND ct.TRANGTHAI NOT IN ('PNCNK','CXN','HUY')
        AND ct.KHONHAP_ID = '$KHODUOC_ID$'
        AND CONVERT(date, CT.NGAYCHUNGTU) BETWEEN   CONVERT(date, '$TUNGAY$')  AND   CONVERT(date, '$DENNGAY$')
        AND ct.BENHVIEN_ID = '$BENHVIEN_ID$'
        GROUP BY ct.KHONHAP_ID, ctct.DUOC_ID, sln.SOLONHAP_ID, sln.SOLONHAP
        )NHAP on TK.KHODUOC_ID = NHAP.KHODUOC_ID AND TK.DUOC_ID = NHAP.DUOC_ID AND TK.SOLONHAP_ID = NHAP.SOLONHAP_ID

        LEFT JOIN
        (
        <!--/*xuất trong kỳ*/-->
        SELECT
        ct.KHOXUAT_ID as KHODUOC_ID, ctct.DUOC_ID, sln.SOLONHAP_ID, sln.SOLONHAP, sum(ctct.SOLUONGDAXUAT) as SL_XUAT
        FROM
        TT_DUOC_CHUNGTU ct
        inner join TT_DUOC_CHUNGTU_CHITIET ctct on ct.CHUNGTU_ID = ctct.CHUNGTU_ID
        inner join TT_DUOC_CHUNGTU_SOLONHAP sln on sln.SOLONHAP_ID = ctct.SOLONHAP_ID
        WHERE
        ct.LOAICHUNGTU = 'X'
        AND ct.KHOXUAT_ID = '$KHODUOC_ID$'
        AND CONVERT(date, CT.NGAYCHUNGTU) BETWEEN   CONVERT(date, '$TUNGAY$')  AND   CONVERT(date, '$DENNGAY$')
        AND ct.BENHVIEN_ID = '$BENHVIEN_ID$'
        GROUP BY ct.KHOXUAT_ID, ctct.DUOC_ID, sln.SOLONHAP_ID, sln.SOLONHAP
        )XUAT on TK.KHODUOC_ID = XUAT.KHODUOC_ID AND TK.DUOC_ID = XUAT.DUOC_ID AND TK.SOLONHAP_ID = XUAT.SOLONHAP_ID

        WHERE TK.TONKHO >= 0
        ORDER BY DUOC_ID, SOLONHAP_ID
      </statement>
