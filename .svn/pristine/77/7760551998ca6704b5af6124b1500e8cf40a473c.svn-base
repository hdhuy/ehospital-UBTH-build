        </statement>
        <statement id="DAO00030_GetKhoDuoc" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
          SELECT *
          FROM
          TM_KHODUOC
          WHERE
          KHODUOC_ID= '$KHODUOC_ID$'
