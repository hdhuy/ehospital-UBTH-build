            <isNotEmpty prepend="and" property="NGUONDUOC_ID">
                NGUONDUOC_ID = '$NGUONDUOC_ID$'
            </isNotEmpty>
            )A,
            (
            SELECT
            ISNULL(SUM(SOLUONG),0) AS SOLUONGBOOKING_NGOAITRU FROM TT_DUOC_TONKHO_BOOK_NGOAITRU
            WHERE
            DUOC_ID = '$DUOC_ID$'
            AND KHODUOC_ID = '$KHODUOC_ID$'
