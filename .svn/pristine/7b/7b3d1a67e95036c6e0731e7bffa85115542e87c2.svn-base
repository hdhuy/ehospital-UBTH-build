    <statement id="DAO00304_GetVienPhiTheoDichVu" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
      select *
      from TT_VIENPHI
      where TIEPNHAN_ID = '$TIEPNHAN_ID$'
