      from TT_VIENPHI vp
      where vp.DOTDIEUTRI_ID IN (SELECT * FROM DBO.SPLIT('$DOTDIEUTRI_ID$',', '))
    </statement>
    <statement id="DAO00304_GetVienPhiWithKhamBenhId" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
