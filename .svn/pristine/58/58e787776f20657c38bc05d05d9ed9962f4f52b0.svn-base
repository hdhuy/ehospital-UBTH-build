    <update id="DAO09800_UpdateBHYT_IDChuyen" parameterClass="System.Collections.IDictionary">
       UPDATE dbo.TT_XACNHANCHIPHI SET IDCHUYEN = #CHUYENTICHHOP_ID# WHERE XACNHANCHIPHI_ID = '$XACNHANCHIPHI_ID$'
