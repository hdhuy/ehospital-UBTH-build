      JOIN dbo.TT_BENHNHAN BN ON TN.BENHNHAN_ID = BN.BENHNHAN_ID
      WHERE   CT.BENHVIEN_ID = '$BENHVIEN_ID$' AND CAST(TN.NGAYTIEPNHAN AS DATE) BETWEEN '$TUNGAY$' AND '$DENNGAY$'
      AND CT.IDCHUYEN IS  NULL
