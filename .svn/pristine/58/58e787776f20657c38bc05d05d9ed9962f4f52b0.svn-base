      JOIN dbo.TM_KHODUOC KD ON KD.KHODUOC_ID = CT.KHOXUAT_ID
      WHERE HDCT.HOADON_ID = hd.HOADON_ID) TT
      WHERE  HD.BENHVIEN_ID = '$BENHVIEN_ID$' AND CAST(HD.NGAYHOADON AS DATE) BETWEEN '$TUNGAY$' AND '$DENNGAY$'
