      GIATRIVAT = NULL ,
      GIATRICHIETKHAU = NULL ,
      CTCT.GIATRITHANHTOAN ,
      GHICHU = NULL,
      LOAI = CT.LOAI ,
      TENLOAI = N'Nội trú' ,
      CT.BENHVIEN_ID ,
      CT.BENHVIEN_ID AS MABENHVIEN ,
