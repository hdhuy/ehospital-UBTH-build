      JOIN VIEW_IDCHUYENTICHHOP TH on TH.CHUYENTICHHOP_ID = CT.IDCHUYEN
      JOIN dbo.TM_KHODUOC KX ON CT.KHOXUAT_ID = KX.KHODUOC_ID
      CROSS APPLY ( SELECT    SUM(CTCT.SOLUONGDAXUAT * SLGB.DONGIAVON) AS GIATRITHANHTOAN
