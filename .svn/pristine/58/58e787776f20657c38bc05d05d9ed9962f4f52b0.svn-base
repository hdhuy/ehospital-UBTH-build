		  </isNotEmpty>
    </statement>
    <!--Lấy danh sách BHYT đã chuyển-->
    <statement id="DAO09800_GetMasterBHYT_DACHUYEN" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
