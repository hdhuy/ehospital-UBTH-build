      <RESULTS>
        <RESULT>
          <KEY>SOTIEPNHAN</KEY>
          <VALUE>@SOTIEPNHAN</VALUE>
        </RESULT>
      </RESULTS>
    </PROCESS>
    <PROCESS>
      <FLOWCONDITIONS>@TAMUNG == '0'</FLOWCONDITIONS>
