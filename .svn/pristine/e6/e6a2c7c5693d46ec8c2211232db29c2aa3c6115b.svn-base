      <NAME>Quy trình nội trú</NAME>
      <CONDITIONS>@SOBENHAN != '' and @HOSPITAL_ID != ''</CONDITIONS>
      <RESULTS>
        <RESULT>
          <KEY>SOTIEPNHAN</KEY>
          <VALUE>@SOTIEPNHAN</VALUE>
        </RESULT>
      </RESULTS>
    
  </PROCESS>
    <PROCESS>
