      <PROCESS_CODE>P01</PROCESS_CODE>
      <NAME>Quy trình ngoại trú trả tiền trước</NAME>
      <CONDITIONS>(@SOTIEPNHAN != '') and (@HOSPITAL_ID != '')</CONDITIONS>
      <RESULTS>
        <RESULT>
          <KEY>SOTIEPNHAN</KEY>
          <VALUE>@SOTIEPNHAN</VALUE>
        </RESULT>
      </RESULTS>
    </PROCESS>
    <PROCESS>
