      , ISNULL(PC.NGUOIDUYETCAP3_ID, ISNULL(PC.NGUOIDUYETCAP2_ID, PC.NGUOIDUYETCAP1_ID)) AS NGUOIDUYETMUAHANG
      from TT_DUOC_DUTRU AS DD
