                <isNotEmpty prepend="and" property="CHUYENKHOA_ID">
                    TM_CHUYENKHOA.CHUYENKHOA_ID = '$CHUYENKHOA_ID$'
                </isNotEmpty>
                <isNotEmpty prepend="and" property="MACHUYENKHOA">
                    TM_CHUYENKHOA.MACHUYENKHOA = '$MACHUYENKHOA$'
                </isNotEmpty>
                <isNotEmpty prepend="and" property="TENCHUYENKHOA">
                    TM_CHUYENKHOA.TENCHUYENKHOA = '$TENCHUYENKHOA$'
                </isNotEmpty>
                <isNotEmpty prepend="and" property="TENKHONGDAU">
                    TM_CHUYENKHOA.TENKHONGDAU = '$TENKHONGDAU$'
                </isNotEmpty>
                <isNotEmpty prepend="and" property="TENCHUYENKHOA_EN">
                    TM_CHUYENKHOA.TENCHUYENKHOA_EN = '$TENCHUYENKHOA_EN$'
                </isNotEmpty>
                <isNotEmpty prepend="and" property="TENCHUYENKHOA_RU">
                    TM_CHUYENKHOA.TENCHUYENKHOA_RU = '$TENCHUYENKHOA_RU$'
