  <statements>
      <statement id="DAO00224_KiemTraCoChiDinhDichVu" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
        SELECT YC.DVYEUCAU_ID
        FROM TT_DVYEUCAU YC
        INNER JOIN TM_DICHVU DV ON YC.DICHVU_ID=DV.DICHVU_ID
        WHERE YC.TIEPNHAN_ID='$TIEPNHAN_ID$' AND DV.MADICHVU='$MADICHVU$'
      </statement>
    <statement id="DAO00224_LayThongTinPhieu" resultClass="DataObject" parameterClass="System.Collections.IDictionary">
      select YC.DVYEUCAU_ID ,NGAYGIOYEUCAU NGAYCHIDINH,YC.CHANDOAN LAMSANG,YC.NGUOIGIOITHIEU_ID,YC.BACSICHIDINH_ID
      , ISNULL(TM.KHAMTIENME_ID,0) KHAMTIENME_ID,YC.NOIYEUCAU_ID,yc.DICHVU_ID
      from TT_DVYEUCAU yc
      LEFT JOIN TT_NOITRU_KHAMTIENME TM ON YC.DVYEUCAU_ID=TM.DVYEUCAU_ID
      where yc.DVYEUCAU_ID='$DVYEUCAU_ID$'
    </statement>
    <statement id="DAO00224_LoadDanhMucPhieu" resultClass="DataObject" parameterClass="System.Collections.IDictionary">
      SELECT YC.SOPHIEUYEUCAU , YC.DVYEUCAU_ID
      FROM TT_DVYEUCAU YC
      LEFT JOIN  TM_DICHVU DM ON YC.DICHVU_ID= DM.DICHVU_ID
      where yc.TIEPNHAN_ID='$TIEPNHAN_ID$'and YC.DICHVU_ID='$DICHVU_ID$' AND YC.NOITHUCHIEN_ID='$NOITHUCHIEN_ID$'
    </statement>
