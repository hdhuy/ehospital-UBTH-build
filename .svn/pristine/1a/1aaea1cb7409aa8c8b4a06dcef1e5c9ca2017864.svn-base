      WITH (
      TENDUOC nvarchar(1000), SOLUONG_BUOISANG numeric(18, 2), SOLUONG_BUOITRUA numeric(18, 2),SOLUONG_BUOICHIEU numeric(18, 2),
      SOLUONG_BUOITOI numeric(18, 2),SOLANTRENNGAY numeric(18, 2),SONGAY numeric(18, 2), SOLUONG numeric(18, 2),DUONGDUNG_ID int,
      DUONGDUNG nvarchar(100), GHICHU nvarchar(1000)) json

      <!--update TT_NOITRUTOATHUOCNGOAI_CHITIET
      set
      TT_NOITRUTOATHUOCNGOAI_CHITIET.TENDUOC = json.TENDUOC,
      TT_NOITRUTOATHUOCNGOAI_CHITIET.SOLUONG_BUOISANG = json.SOLUONG_BUOISANG,
