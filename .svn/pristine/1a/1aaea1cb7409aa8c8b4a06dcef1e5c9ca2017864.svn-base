    </statement>
    <statement id="DAO00079_GetTT_GiayRaVien_KUB" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
      declare @PhauThuat nvarchar(2000) = ''
      select @PhauThuat = @PhauThuat + DV.TENDICHVU + '; '
      from TT_DVYEUCAU YC
      inner join TM_DICHVU DV on DV.DICHVU_ID = YC.DICHVU_ID and DV.BENHVIEN_ID = YC.BENHVIEN_ID
      inner join TM_NHOMDICHVU NDV on NDV.NHOMDICHVU_ID = DV.NHOMDICHVU_ID and NDV.BENHVIEN_ID = DV.BENHVIEN_ID
      where YC.BENHAN_ID = $BENHAN_ID$
      and SUBSTRING(NDV.MANHOMDICHVU, 1, 2) = '05'
      and YC.BENHVIEN_ID = '$BENHVIEN_ID$'
      and YC.HUYYEUCAU = '0'

      declare @HoaChat nvarchar(2000) = ''
      select @HoaChat = @HoaChat + D.TENDUOCDAYDU + '; '
      from TT_NOITRU_KHAMBENH KB
      inner join TT_NOITRU_TOATHUOC TT on TT.KHAMBENH_ID = KB.KHAMBENH_ID and TT.BENHVIEN_ID = KB.BENHVIEN_ID
      inner join TM_DUOC D on D.DUOC_ID = TT.DUOC_ID and D.BENHVIEN_ID = TT.BENHVIEN_ID
      inner join TM_LOAIDUOC LD on LD.LOAIDUOC_ID = D.LOAIDUOC_ID and LD.BENHVIEN_ID = D.BENHVIEN_ID
      where KB.BENHAN_ID = $BENHAN_ID$
      and LD.MALOAIDUOC in ('L006', 'L007')
      and KB.BENHVIEN_ID = '$BENHVIEN_ID$'
      and TT.HUYTOATHUOC = '0'

      select BA.BENHAN_ID, BA.TIEPNHAN_ID, BA.BENHNHAN_ID, GRV.CHANDOANMOBENHHOC, ISNULL(GRV.PPDT_PHAUTHUAT, @PhauThuat) AS PPDT_PHAUTHUAT, GRV.PPDT_TIAXA,
      ISNULL(GRV.PPDT_HOACHAT, @HoaChat) AS PPDT_HOACHAT, GRV.PPDT_KHAC, ISNULL(GRV.KETQUADIEUTRI_MOTA, KQ.DICTIONARY_NAME) AS KETQUADIEUTRI_MOTA, BA.KETQUADIEUTRI_ID,
      GRV.CHEDOSAURAVIEN, ISNULL(GRV.BACSIDIEUTRI_ID, BA.BACSIDIEUTRI_ID) AS BACSIDIEUTRI_ID, GRV.THUTRUONGDONVI_ID, GRV.GHICHU, GRV.GIAYRAVIEN_ID
      from TT_NOITRU_BENHAN BA
      left join TT_NOITRU_GIAYRAVIEN_KUB GRV on GRV.BENHAN_ID = BA.BENHAN_ID and GRV.BENHVIEN_ID = BA.BENHVIEN_ID
      left join LST_DICTIONARY KQ on KQ.DICTIONARY_ID = BA.KETQUADIEUTRI_ID
      where BA.BENHAN_ID = $BENHAN_ID$
      and BA.BENHVIEN_ID = '$BENHVIEN_ID$'
    </statement>
    <update id="DAO00079_TT_NOITRU_GIAYRAVIEN_KUB_Update" parameterClass="System.Collections.IDictionary">
      UPDATE TT_NOITRU_GIAYRAVIEN_KUB
      SET
      CHANDOANMOBENHHOC = #CHANDOANMOBENHHOC#
      ,PPDT_PHAUTHUAT = #PPDT_PHAUTHUAT#
      ,PPDT_TIAXA = #PPDT_TIAXA#
      ,PPDT_HOACHAT = #PPDT_HOACHAT#
      ,PPDT_KHAC = #PPDT_KHAC#
      ,KETQUADIEUTRI_MOTA = #KETQUADIEUTRI_MOTA#
      ,CHEDOSAURAVIEN = #CHEDOSAURAVIEN#
      ,BACSIDIEUTRI_ID = #BACSIDIEUTRI_ID#
      ,THUTRUONGDONVI_ID = #THUTRUONGDONVI_ID#
      ,GHICHU = #GHICHU#
      ,NGAYCAPNHAT = #NGAYCAPNHAT#
      ,NGUOICAPNHAT_ID = #NGUOICAPNHAT_ID#
      WHERE GIAYRAVIEN_ID = $GIAYRAVIEN_ID$
    </update>
    <statement id="DAO00079_TT_NOITRU_GIAYRAVIEN_KUB_Add" parameterClass="System.Collections.IDictionary" resultClass="System.Int64">
      INSERT INTO TT_NOITRU_GIAYRAVIEN_KUB
      (BENHNHAN_ID
      ,TIEPNHAN_ID
      ,BENHAN_ID
      ,CHANDOANMOBENHHOC
      ,PPDT_PHAUTHUAT
      ,PPDT_TIAXA
      ,PPDT_HOACHAT
      ,PPDT_KHAC
      ,KETQUADIEUTRI_MOTA
      ,CHEDOSAURAVIEN
      ,BACSIDIEUTRI_ID
      ,THUTRUONGDONVI_ID
      ,GHICHU
      ,BENHVIEN_ID
      ,NGAYTAO
      ,NGUOITAO_ID
      ) VALUES (
      #BENHNHAN_ID#
      ,#TIEPNHAN_ID#
      ,#BENHAN_ID#
      ,#CHANDOANMOBENHHOC#
      ,#PPDT_PHAUTHUAT#
      ,#PPDT_TIAXA#
      ,#PPDT_HOACHAT#
      ,#PPDT_KHAC#
      ,#KETQUADIEUTRI_MOTA#
      ,#CHEDOSAURAVIEN#
      ,#BACSIDIEUTRI_ID#
      ,#THUTRUONGDONVI_ID#
      ,#GHICHU#
      ,#BENHVIEN_ID#
      ,#NGAYTAO#
      ,#NGUOITAO_ID#
      )
      SELECT SCOPE_IDENTITY()
    </statement>
    <statement id="DAO00079_TT_NOITRU_GIAYRAVIEN_KUB_Delete" parameterClass="System.Collections.IDictionary" resultClass="System.Int32">
      delete from TT_NOITRU_GIAYRAVIEN_KUB
      where GIAYRAVIEN_ID = $GIAYRAVIEN_ID$
    </statement>
    <statement id="DAO00079_CheckDaGhiNhanVTYT" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
      SELECT *
      FROM TT_CLSKETQUA_VTYT
      WHERE DVYEUCAU_ID = '$DVYEUCAU_ID$' AND BENHVIEN_ID = '$BENHVIEN_ID$'
    </statement>
    <statement id="DAO00079_GetAllYLenhThuoc" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
      /*Y lệnh thuốc chưa ký*/
      select a.KHAMBENH_ID
      , a.THOIGIANKHAM
      , a.PHONGBAN_ID
      , b.TOATHUOC_ID
      , b.SOLUONG
      , d.DUOC_ID
      , d.TENDUOCDAYDU
      , d.DONVITINH
      , d.DUONGDUNG_ID
      , c.BENHNHAN_ID
      , NULL as CHUKY
      , a.BENHAN_ID
      from TT_NOITRU_KHAMBENH a
      join TT_NOITRU_TOATHUOC b on a.KHAMBENH_ID = b.KHAMBENH_ID
      join TT_NOITRU_BENHAN c on a.BENHAN_ID = c.BENHAN_ID
      join TM_DUOC d on d.DUOC_ID = b.DUOC_ID
      where NOT EXISTS (select KHAMBENH_ID from TT_CHUKY_THUOC where TT_CHUKY_THUOC.KHAMBENH_ID = a.KHAMBENH_ID)
      and a.ISYLENHVTYT = '0'
      and c.TRANGTHAI = 'DANGDIEUTRI'
      and a.BENHAN_ID = '$BENHAN_ID$'
      and a.BENHVIEN_ID = '$BENHVIEN_ID$'

      /*Y lệnh thuốc đã ký*/
      union all
      select a.KHAMBENH_ID
      , a.THOIGIANKHAM
      , a.PHONGBAN_ID
      , b.TOATHUOC_ID
      , b.SOLUONG
      , b.DUOC_ID
      , c.TENDUOCDAYDU
      , c.DONVITINH
      , c.DUONGDUNG_ID
      , a.BENHNHAN_ID
      , a.CHUKY
      , a.BENHAN_ID
      from TT_CHUKY_THUOC a
      join TT_CHUKY_YLENHTHUOC b on a.CHUKY_ID = b.CHUKY_ID
      join TM_DUOC c on c.DUOC_ID = b.DUOC_ID
      where a.BENHVIEN_ID = '$BENHVIEN_ID$'
      and a.BENHAN_ID = '$BENHAN_ID$'
      order by a.KHAMBENH_ID
    </statement>
    <statement id="DAO00079_AddChuKyYlenhThuoc" parameterClass="System.Collections.IDictionary" resultClass="System.Int64">
      DECLARE @ChuKy_ThuocJson NVARCHAR(max) = N'$CHUKY_THUOC$';
      DECLARE @ChuKy_YLenh_ThuocJson NVARCHAR(max) = N'$CHUKY_YLENH_THUOC$';
      DECLARE @chuky_id INT;
      SET @chuky_id = 0;

      DECLARE @ChuKy_Thuoc TABLE ([KhamBenh_Id] INT, [BenhNhan_Id] INT, [PhongBan_Id] INT, [NguoiDuaKy] INT, [ChuKy] VARCHAR(max), [BenhAn_Id] INT, ThoiGianKham DATETIME)
      DECLARE @ChuKy_YLenh_Thuoc TABLE ([ToaThuoc_Id]	INT, [Duoc_Id] INT, [SoLuong] DECIMAL(18,4), [DuongDung_Id]	INT)

      INSERT INTO @ChuKy_Thuoc
      SELECT * FROM OPENJSON(@ChuKy_ThuocJson)
      WITH
      (
      [KhamBenh_Id] INT
      , [BenhNhan_Id] INT
      , [PhongBan_Id] INT
      , [NguoiDuaKy] INT
      , [ChuKy] VARCHAR(MAX)
      , [BenhAn_Id] INT
      , [ThoiGianKham] DATETIME
      )

      INSERT INTO @ChuKy_YLenh_Thuoc
      SELECT * FROM OPENJSON(@ChuKy_YLenh_ThuocJson)
      WITH
      (
      [ToaThuoc_Id] INT
      , [Duoc_Id] INT
      , [SoLuong] DECIMAL(18,4)
      , [DuongDung_Id] INT
      )


      IF EXISTS(SELECT TOP 1 1 FROM @ChuKy_Thuoc)
      INSERT INTO TT_CHUKY_THUOC
      (
      [KHAMBENH_ID], [BENHNHAN_ID], [PHONGBAN_ID], [NGUOIDUAKY], [CHUKY], [BENHAN_ID], [THOIGIANKHAM], [NGAYKY], [BENHVIEN_ID]
      )
      SELECT *, getdate() NgayKy, '$BENHVIEN_ID$' FROM @ChuKy_Thuoc

      SET @chuky_id = (select SCOPE_IDENTITY())


      IF EXISTS(SELECT TOP 1 1 FROM @ChuKy_YLenh_Thuoc)
      INSERT INTO TT_CHUKY_YLENHTHUOC([CHUKY_ID], [TOATHUOC_ID], [DUOC_ID], [SOLUONG], [DUONGDUNG_ID], [BENHVIEN_ID])
      SELECT @chuky_id, *, '$BENHVIEN_ID$' FROM @ChuKy_YLenh_Thuoc
      SELECT @chuky_id
    </statement>
    <statement id="DAO00079_GetDieuTriTamLy" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
      select a.*, b.TENNHANVIEN as NHATRILIEU
      from TT_NOITRU_DIEUTRITAMLY a
      left join TM_NHANVIEN b on a.BACSITRILIEU_ID = b.NHANVIEN_ID
      where BENHAN_ID = '$BENHAN_ID$'
    </statement>
    <statement id="DAO00079_GetDieuTriTamLy_SoKet" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
      select a.*,(N'Lần ' + cast(a.LAN as nvarchar(100))) as LAN_TEXT , b.TENNHANVIEN as NGUOICANTHIEP
      from TT_NOITRU_DIEUTRITAMLY_SOKET a
      join TM_NHANVIEN b on a.NGUOICANTHIEP_ID = b.NHANVIEN_ID
      where a.BENHAN_ID = '$BENHAN_ID$'
    </statement>
    <statement id="DAO00079_GetDieuTriTamLy_LamSang" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
      select *
      from TT_NOITRU_DIEUTRITAMLY_LAMSANG
      where BENHAN_ID = '$BENHAN_ID$'
    </statement>
    <statement id="DAO00079_GetChuyenKhoa_TamThan" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
      select *
      from TT_NOITRU_CHUYENKHOA
      where BENHNHAN_ID = '$BENHNHAN_ID$'
    </statement>
    <statement id="DAO00079_GetQuanHuyen" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
      select * from TM_DONVIHANHCHINH where CAPDONVI = '3'
    </statement>
    <statement id="DAO00079_GetXaPhuong" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
      select * from TM_DONVIHANHCHINH where CAPDONVI = '4'
    </statement>
    <statement id="DAO00079_GetBenhAnMau" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
      select * from TT_NOITRU_BENHAN_MAU
    </statement>
    <statement id="DAO00079_GetTomTat_SoKet_ChungNhan" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
      select * from $TABLE$ where BENHAN_ID = '$BENHAN_ID$'
    </statement>
    <statement id="DAO00079_GetTomTat_ChungNhan" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
      select a.BENHAN_ID, a.CHANDOANKHAMBENH as CHANDOANVAOVIEN, (isnull(a.CDRAVIEN_CHINH,'') + ' ' + isnull(a.CDRAVIEN_PHU,'')) as CHANDOANRAVIEN
      , b.TOMTATBENHAN, b.PPDT as PHUONGPHAPDIEUTRI, b.TINHTRANGNGUOIBENHRAVIEN as TINHTRANGRAVIEN
      , b.LYDOVAOVIEN, b.QUATRINHBENHLY as QUATRINHBENHTAT, b.PPDT as DAXUTRI_DIEUTRI
      , (isnull(a.CDRAVIEN_CHINH,'') + ' ' + isnull(a.CDRAVIEN_PHU,'')) as CHANDOAN_KETLUAN
      from TT_NOITRU_BENHAN a
      left join TT_NOITRU_BENHAN_CHITIET b on a.BENHAN_ID = b.BENHAN_ID
      where a.BENHAN_ID = '$BENHAN_ID$'
    </statement>
    <statement id="DAO00079_GetKhamTienMe" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
      select top 1 * from TT_NOITRU_KHAMTIENME where BENHVIEN_ID = '$BENHVIEN_ID$' order by KHAMTIENME_ID desc
    </statement>
</statements>
