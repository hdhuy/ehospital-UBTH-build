      TT_NOITRUTOATHUOCNGOAI_CHITIET.SOLUONG_BUOICHIEU = json.SOLUONG_BUOICHIEU,
      TT_NOITRUTOATHUOCNGOAI_CHITIET.SOLUONG_BUOITOI = json.SOLUONG_BUOITOI,
      TT_NOITRUTOATHUOCNGOAI_CHITIET.SOLANTRENNGAY = json.SOLANTRENNGAY,
      TT_NOITRUTOATHUOCNGOAI_CHITIET.SONGAY = json.SONGAY,
      TT_NOITRUTOATHUOCNGOAI_CHITIET.SOLUONG = json.SOLUONG,
      TT_NOITRUTOATHUOCNGOAI_CHITIET.DUONGDUNG_ID = json.DUONGDUNG_ID,
      TT_NOITRUTOATHUOCNGOAI_CHITIET.DUONGDUNG = json.DUONGDUNG,
      TT_NOITRUTOATHUOCNGOAI_CHITIET.GHICHU = json.GHICHU
      FROM OPENJSON(@jsonData)
      WITH (TOATHUOCCHITIET_ID int,
      TENDUOC nvarchar(1000), SOLUONG_BUOISANG numeric(18, 2), SOLUONG_BUOITRUA numeric(18, 2),SOLUONG_BUOICHIEU numeric(18, 2),
      SOLUONG_BUOITOI numeric(18, 2),SOLANTRENNGAY numeric(18, 2),SONGAY numeric(18, 2), SOLUONG numeric(18, 2),DUONGDUNG_ID int,
      DUONGDUNG nvarchar(100), GHICHU nvarchar(1000)) json
      where TT_NOITRUTOATHUOCNGOAI_CHITIET.TOATHUOCCHITIET_ID = json.TOATHUOCCHITIET_ID-->
