    <statement id="DAO00079_UpdateBacsiBenhnhan" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
      IF EXISTS
      (SELECT BACSIDIEUTRI_BENHNHAN_ID FROM TT_BACSIDIEUTRI_BENHNHAN WHERE BACSIDIEUTRI_ID = #BACSIDIEUTRI_ID# and BENHNHAN_ID = #BENHNHAN_ID#)
      BEGIN
      update TT_BACSIDIEUTRI_BENHNHAN set
      NGAYCAPNHAT = getdate(),
      NGUOICAPNHAT_ID = #NGUOITAO_ID#
      <isEqual prepend="," property="KHAMBENH_NGOAITRU" compareValue="1">
        KHAMBENH_NGOAITRU = '1'
      </isEqual>
      <isEqual prepend="," property="HOSOBENHAN_NOITRU" compareValue="1">
        HOSOBENHAN_NOITRU = '1'
      </isEqual>
      WHERE BACSIDIEUTRI_ID = #BACSIDIEUTRI_ID# and BENHNHAN_ID = #BENHNHAN_ID#
      END
      ELSE
      BEGIN
      insert into TT_BACSIDIEUTRI_BENHNHAN (BACSIDIEUTRI_ID, BENHNHAN_ID, KHAMBENH_NGOAITRU,
      HOSOBENHAN_NOITRU, BENHVIEN_ID, NGAYTAO, NGUOITAO_ID)
      values (#BACSIDIEUTRI_ID#, #BENHNHAN_ID#, '$KHAMBENH_NGOAITRU$', '$HOSOBENHAN_NOITRU$', #BENHVIEN_ID#,
      getdate(), #NGUOITAO_ID#)
      END
    </statement>
    <statement id="DAO00079_CheckExistBacsiBenhnhan" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
      SELECT BACSIDIEUTRI_BENHNHAN_ID FROM TT_BACSIDIEUTRI_BENHNHAN WHERE BACSIDIEUTRI_ID = #BACSIDIEUTRI_ID# and BENHNHAN_ID = #BENHNHAN_ID#
    </statement>
    <statement id="DAO00079_Canhbaothuocsaphethan" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
      select dtk.SOLUONG, dc.HANDUNG, dtk.DUOC_ID
      from TT_DUOC_TONKHO dtk
      inner join TT_DUOC_CHUNGTU_SOLONHAP dc on dc.SOLONHAP_ID = dtk.SOLONHAP_ID
      where dtk.SOLUONG > 0
      and dc.HANDUNG >= GETDATE()
      and dateadd(dd,$SONGAY_CANHBAO$,cast(getdate() as date)) >= dc.HANDUNG
      and dtk.DUOC_ID = $DUOC_ID$
    </statement>
