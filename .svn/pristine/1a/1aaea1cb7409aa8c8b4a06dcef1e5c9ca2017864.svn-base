      insert into TT_NOTTRUTOATHUOCNGOAI
      SELECT $BENHNHAN_ID$, $TIEPNHAN_ID$, $BENHAN_ID$, getdate(), '$BENHVIEN_ID$',
      $NGUOITAO_ID$, GETDATE(), $NGUOITAO_ID$, GETDATE(), $KHAMBENH_ID$, '$LOAITOA$', '$LOIDANBACSI$', '$SONGAYHEN$'
      set @toathuocngoai_id = SCOPE_IDENTITY()

      insert into TT_NOITRUTOATHUOCNGOAI_CHITIET
      SELECT @toathuocngoai_id, $BENHNHAN_ID$, $TIEPNHAN_ID$, $BENHAN_ID$, json.TENDUOC, json.SOLUONG_BUOISANG, json.SOLUONG_BUOITRUA,
      json.SOLUONG_BUOICHIEU, json.SOLUONG_BUOITOI,json.SOLANTRENNGAY, json.SONGAY,json.SOLUONG, json.DUONGDUNG_ID, json.DUONGDUNG,
      json.GHICHU, 0,'$BENHVIEN_ID$',$NGUOITAO_ID$, GETDATE(), $NGUOITAO_ID$, GETDATE()
      FROM OPENJSON(@jsonData)
      WITH (
      TENDUOC nvarchar(1000), SOLUONG_BUOISANG numeric(18, 2), SOLUONG_BUOITRUA numeric(18, 2),SOLUONG_BUOICHIEU numeric(18, 2),
      SOLUONG_BUOITOI numeric(18, 2),SOLANTRENNGAY numeric(18, 2),SONGAY numeric(18, 2), SOLUONG numeric(18, 2),DUONGDUNG_ID int,
      DUONGDUNG nvarchar(100), GHICHU nvarchar(1000)) json
    </statement>
