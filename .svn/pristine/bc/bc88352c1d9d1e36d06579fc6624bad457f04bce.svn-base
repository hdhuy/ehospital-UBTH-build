      , NGAYCAPNHAT = #NGAYCAPNHAT#
      , NGUOICAPNHAT_ID = #NGUOICAPNHAT_ID#
      where
      KHOMAU_ID = #KHOMAU_ID#
      AND MAU_ID = #MAU_ID#
      AND  SOLONHAP_ID = #SOLONHAP_ID#
      END
      ELSE
      BEGIN
      insert into TT_MAU_TONKHO (
      KHOMAU_ID
      , MAU_ID
      , SOLONHAP_ID
      , SOLUONG
      , NGUONHANG_ID
      , MAU_KEY
      , BENHVIEN_ID
      , NGAYTAO
      , NGUOITAO_ID
      , NGAYCAPNHAT
