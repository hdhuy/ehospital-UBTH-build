      <dynamic prepend="where">
        <isParameterPresent>
          <isNotEmpty prepend="and" property="CHUNGTUCHITIET_ID">
            TT_MAU_CHUNGTU_CHITIET.CHUNGTUCHITIET_ID = '$CHUNGTUCHITIET_ID$'
