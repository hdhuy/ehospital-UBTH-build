      SELECT TONKHO_ID
      FROM TT_MAU_TONKHO
      WHERE
      KHOMAU_ID = #KHOMAU_ID#
      AND MAU_ID = #MAU_ID#
      AND  SOLONHAP_ID = #SOLONHAP_ID#
      )
      BEGIN
      update TT_MAU_TONKHO  set
