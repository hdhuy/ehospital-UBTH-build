      UPDATE TT_MAU_DANGKYMAU_CHITIET SET CHUNGTU_ID = null, TRANGTHAI = 'CHUAXUAT', NGAYCAPNHAT = getdate() WHERE CHUNGTU_ID = '$CHUNGTU_ID$'

