      AND NOT EXISTS (SELECT * FROM TT_MAU_KHOASOCHUNGTU WHERE KHOMAU_ID = '$KHOMAU_ID$' and BENHVIEN_ID = '$BENHVIEN_ID$')
      ORDER BY NGAYCHUNGTU ASC
    </statement>
