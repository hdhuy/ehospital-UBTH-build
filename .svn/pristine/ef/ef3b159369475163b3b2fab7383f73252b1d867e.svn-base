      <statement id="DAO00200_GetPermissionCategorySystem" resultClass="DataObject">
        SELECT DISTINCT C.CATEGORY_ID AS REPORT_ID
        , ISNULL(C.PARENT_ID,0) AS PARENT_ID
        , C.CATEGORY_CODE AS REPORT_CODE
        , C.CATEGORY_NAME AS REPORT_NAME
        , C.FOLDER_LEVEL
        , C.IS_FOLDER
        , C.FOLDER_CODE
        , CASE WHEN (C.FOLDER_CODE IS NULL OR C.FOLDER_CODE = 'ROOT' OR C.FOLDER_CODE = '' )
        THEN NULL ELSE C.FOLDER_CODE END AS PARENT_CODE
        , C.CATEGORY_TYPE
        , C.CATEGORY_NAME as DESCRIPTION
        FROM TM_CATEGORY AS C
        WHERE C.ENABLED = '1'
        AND C.BENHVIEN_ID = '$BENHVIEN_ID$' AND C.CATEGORY_ID != 0
        ORDER BY C.CATEGORY_ID
      </statement>
    </statements>
</sqlMap>
