        <statement id="DAO00200_GetAllWithCodeAndParentID" resultClass="DataObject">
            SELECT R.REPORT_ID AS REPORT_ID, ISNULL(RS.REPORT_ID,0) AS PARENT_ID, R.REPORT_CODE, R.REPORT_NAME, R.FOLDER_LEVEL, R.IS_FOLDER, R.FOLDER_CODE
            FROM TM_REPORT AS R
            LEFT JOIN TM_REPORT_SUB_DEFINITION AS RS ON R.REPORT_CODE = RS.SUB_REPORT_CODE AND RS.BENHVIEN_ID = R.BENHVIEN_ID
            WHERE R.REPORT_ID != 0 AND R.BENHVIEN_ID = '$BENHVIEN_ID$'
            ORDER BY R.REPORT_NAME
        </statement>
        <!--Revision:1,341f03af79827d34d0b9205598682777-->
        <statement id="DAO00200_GetAllPhanQuyen" resultClass="DataObject">
            SELECT R.REPORT_ID AS REPORT_ID, ISNULL(RS.REPORT_ID,-1) AS PARENT_ID, R.REPORT_CODE, R.REPORT_NAME, R.FOLDER_LEVEL, R.IS_FOLDER, R.FOLDER_CODE, R.DESCRIPTION
            , R2.REPORT_CODE AS PARENT_CODE, TAMNGUNG = CASE WHEN R.ENABLED = '1' THEN '0' ELSE '1' END
            FROM TM_REPORT AS R
            LEFT JOIN TM_REPORT_SUB_DEFINITION AS RS ON
            (SELECT TOP 1 REPORT_SUB_DEFINITION_ID FROM TM_REPORT_SUB_DEFINITION WHERE R.REPORT_CODE = SUB_REPORT_CODE AND BENHVIEN_ID = '$BENHVIEN_ID$') = RS.REPORT_SUB_DEFINITION_ID AND RS.BENHVIEN_ID = R.BENHVIEN_ID
            LEFT JOIN TM_REPORT AS R2 ON R2.REPORT_CODE = RS.REPORT_CODE AND R2.BENHVIEN_ID = RS.BENHVIEN_ID
            WHERE R.REPORT_ID != 0 AND R.ENABLED = '1' AND R.BENHVIEN_ID = '$BENHVIEN_ID$' AND R.IS_PHIEU != '1'
            AND (RS.REPORT_ID IS NULL OR R2.IS_FOLDER = '1')
            ORDER BY R.REPORT_NAME
        </statement>
        <!--Revision:0,1c825fdae67f986a15a2a571b98e5721-->
        <statement id="DAO00200_GetListWithoutFolder" resultClass="DataObject">
            WITH ReportRec AS
            (
            SELECT REPORT_CODE FROM TM_REPORT WHERE REPORT_CODE = '$REPORT_CODE$' AND BENHVIEN_ID = '$BENHVIEN_ID$'
            UNION ALL
            SELECT RS.REPORT_CODE
            FROM ReportRec AS RR
            INNER JOIN dbo.TM_REPORT_SUB_DEFINITION AS RS ON RS.SUB_REPORT_CODE = RR.REPORT_CODE AND RS.BENHVIEN_ID = '$BENHVIEN_ID$'
            )
            SELECT DISTINCT R.REPORT_ID, R.REPORT_CODE, R.REPORT_NAME, R.FOLDER_LEVEL, R.IS_FOLDER, R.FOLDER_CODE
            FROM TM_REPORT AS R
            LEFT JOIN TM_REPORT_SUB_DEFINITION AS RS ON R.REPORT_ID = RS.SUB_REPORT_ID AND RS.BENHVIEN_ID = '$BENHVIEN_ID$'
            INNER JOIN TM_REPORT AS R1 ON R1.REPORT_CODE = '$REPORT_CODE$' AND R1.BENHVIEN_ID = '$BENHVIEN_ID$'
            WHERE R.REPORT_ID != 0 AND ((R1.IS_FOLDER = '0' AND R.IS_FOLDER = '0') OR (R1.IS_FOLDER = '1'))
            AND R.REPORT_CODE != '$REPORT_CODE$' AND R.BENHVIEN_ID = '$BENHVIEN_ID$' AND R.REPORT_CODE NOT IN (SELECT DISTINCT REPORT_CODE FROM ReportRec)
            ORDER BY R.REPORT_NAME
        </statement>
