		<statement id="DAO00200_GetSettingReportPrinting" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
        SELECT DICTIONARY_CODE AS REPORT_CODE, DICTIONARY_NAME AS COMPUTER_NAME, DICTIONARY_NAME_EN AS NAME_PRINTER
        FROM LST_DICTIONARY
        WHERE BENHVIEN_ID = '$BENHVIEN_ID$' AND ENABLED = '1' AND DICTIONARY_TYPE_CODE = 'Report_Printer_Setting'
        AND DICTIONARY_CODE = '$REPORT_CODE$' AND DICTIONARY_NAME = '$COMPUTER_NAME$'
      </statement>
