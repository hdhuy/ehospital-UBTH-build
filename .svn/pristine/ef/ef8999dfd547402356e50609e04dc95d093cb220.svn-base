        <statement id="DAO00061_GetSoBenhAnWithTiepNhan" resultClass="DataObject">
          select *
          from TT_NOITRU_BENHAN
          where TIEPNHAN_ID = '$TIEPNHAN_ID$'
          and BENHVIEN_ID = '$BENHVIEN_ID$'
          and right(SOBENHAN,2) != 'CC'
          order by BENHAN_ID
        </statement>
        <!--Revision:0,a7b35a16623e1fbbd55e8785ec9c244d-->
        <statement id="DAO00061_GetDSBenhAnByBenhNhanID" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
      select ntba.*
      ,case
      when SOBENHAN like '%/CC' then icd_cc.MAICD
      else icd_nt.MAICD + COALESCE(';' + ICD_BENHPHU,'')
      end as MAICD
      , case
      when SOBENHAN like '%/CC' then icd_cc.TENICD + COALESCE( '; ' + CHANDOANRAVIEN, '')
      else icd_nt.TENICD + COALESCE('; ' + CHANDOANRAVIEN,'') + (select '; ' + TENICD from [dbo].[SplitString](ICD_BENHPHU,';') as a
      left join TM_ICD icd_p on a.s=icd_p.MAICD
      group by TENICD
      FOR XML PATH ('')
      )
      end as CHANDOAN
      from TT_NOITRU_BENHAN ntba
      left join TM_ICD icd_nt on ntba.ICD_BENHCHINH = icd_nt.ICD_ID
      left join TM_ICD icd_cc on ntba.ICD_KHAMBENH = icd_cc.ICD_ID
      where BENHNHAN_ID = '$BENHNHAN_ID$'
      and ntba.BENHVIEN_ID = '$BENHVIEN_ID$'
      <isNotEmpty prepend="and" property="TUNGAY">
        convert(date, ntba.NGAYTAO) &gt;= convert(date, '$TUNGAY$')
      </isNotEmpty>
      <isNotEmpty prepend="and" property="DENNGAY">
        convert(date, ntba.NGAYTAO) &lt;= convert(date, '$DENNGAY$')
      </isNotEmpty>
      <isNotEmpty prepend="and" property="BACSIKHAM_ID">
        BACSIDIEUTRICHINH_ID = '$BACSIKHAM_ID$'
      </isNotEmpty>
      <isNotEmpty prepend="and" property="PHONGBAN_ID">
        isnull(KHOACHUYENDEN_ID, KHOAVAO_ID) = '$PHONGBAN_ID$'
      </isNotEmpty>
      order by BENHAN_ID desc
    </statement>
