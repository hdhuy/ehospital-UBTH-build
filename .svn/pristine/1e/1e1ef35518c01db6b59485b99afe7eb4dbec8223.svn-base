    <statement id="DAO00212_DEP02_AddByJson" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
      declare @jsonDichVu nvarchar(max) = N'$JSON$';

      DELETE FROM TM_PHONGBAN_DICHVU
      where concat(PHONGBAN_ID, DICHVU_ID)
      IN (SELECT concat(PHONGBAN_ID, DICHVU_ID)
      FROM OPENJSON(@jsonDichVu)
      WITH (PHONGBAN_ID int, DICHVU_ID int)
      )

      INSERT INTO TM_PHONGBAN_DICHVU
      SELECT *
      FROM OPENJSON(@jsonDichVu)
      WITH (
      PHONGBAN_ID int,
      DICHVU_ID int,
      CHIPHI int,
      NGAYTAO datetime,
      NGUOITAO_ID int,
      BENHVIEN_ID varchar(10))
    </statement>
    <statement id="DAO00212_SVC05_AddByJson" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
