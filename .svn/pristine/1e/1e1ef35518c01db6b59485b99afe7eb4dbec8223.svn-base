    </statement>
    <statement id="DAO00212_ETT08_LOAIGIA_GetList" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
      SELECT LG.*, LG2.LOAIGIA_ID AS NHOM_ID, LG2.TENLOAIGIA AS TENNHOM, DLG.DOUUTIEN
      , DLG.NGUONKHACHHANG_BENHNHAN_ID, SELECTED = CASE ISNULL(DLG.NGUONKHACHHANG_BENHNHAN_ID,-1) WHEN -1 THEN 0 ELSE 1 END
      FROM TM_LOAIGIA AS LG
      INNER JOIN TM_LOAIGIA AS LG2 ON LG.CAPTREN_ID = LG2.LOAIGIA_ID AND LG2.BENHVIEN_ID = '$BENHVIEN_ID$' AND LG2.TAMNGUNG = '0'
      LEFT JOIN TM_NGUONKHACHHANG_LOAIGIA AS DLG ON DLG.LOAIGIA_ID = LG.LOAIGIA_ID and DLG.NGUONKHACHHANG_BENHNHAN_ID = '$NGUONKHACHHANG_BENHNHAN_ID$'
      WHERE LG.CAPTREN_ID IS NOT NULL AND LG.TAMNGUNG = '0' AND LG.BENHVIEN_ID = '$BENHVIEN_ID$'
      ORDER BY LG2.IDX
    </statement>
