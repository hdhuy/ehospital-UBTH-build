    <statement id="DAO00212_DEP_KHO_GetList" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
      IF '$ONLYINVENTORY$' = '0'
      BEGIN
      SELECT KHO_ID AS CATEGORY_ID, MAKHO AS CATEGORY_CODE, TENKHO AS CATEGORY_NAME, 1 AS FOLDER_LEVEL, B.DICTIONARY_ID AS PARENT_ID, '0' AS IS_FOLDER
      FROM TM_KHO A
      JOIN LST_DICTIONARY B ON A.MODULE_ID = B.DICTIONARY_ID
      WHERE A.TAMNGUNG = '0' AND A.BENHVIEN_ID = '$BENHVIEN_ID$'
      UNION ALL
      SELECT DICTIONARY_ID AS CATEGORY_ID, DICTIONARY_CODE AS CATEGORY_CODE, DICTIONARY_NAME AS CATEGORY_NAME
      , 0 AS FOLDER_LEVEL, 0 AS PARENT_ID, '0' AS IS_FOLDER
      FROM LST_DICTIONARY
      WHERE DICTIONARY_TYPE_CODE = 'PhanLoaiKho'
      END
      ELSE
      BEGIN
      SELECT KHO_ID AS CATEGORY_ID, MAKHO AS CATEGORY_CODE, TENKHO AS CATEGORY_NAME, 0 AS FOLDER_LEVEL, 0 AS PARENT_ID, '0' AS IS_FOLDER
      FROM TM_KHO A
      JOIN LST_DICTIONARY B ON A.MODULE_ID = B.DICTIONARY_ID
      WHERE A.TAMNGUNG = '0' AND A.BENHVIEN_ID = '$BENHVIEN_ID$'
      <isNotEmpty prepend="and" property="MODULE_CODE">
        B.DICTIONARY_CODE = '$MODULE_CODE$'
      </isNotEmpty>
      END
    </statement>
    <statement id="DAO00212_DEP_PHONGBAN_GetList" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
      WITH PhongbanRec AS
      (
      SELECT P.PHONGBAN_ID AS CATEGORY_ID, P.MAPHONGBAN AS CATEGORY_CODE, P.TENPHONGBAN AS CATEGORY_NAME, P.CAP AS FOLDER_LEVEL, ISNULL(P.CAPTREN_ID,0) AS PARENT_ID, '0' AS IS_FOLDER, P.LOAIPHONGBAN_ID
      FROM TM_PHONGBAN AS P
      WHERE P.CAP = 1 AND ISNULL(P.CAPTREN_ID,0) = 0 AND P.BENHVIEN_ID = '$BENHVIEN_ID$' AND P.TAMNGUNG = '0'
      UNION ALL
      SELECT P.PHONGBAN_ID AS CATEGORY_ID, P.MAPHONGBAN AS CATEGORY_CODE, P.TENPHONGBAN AS CATEGORY_NAME, P.CAP AS FOLDER_LEVEL, P.CAPTREN_ID AS PARENT_ID, '0' AS IS_FOLDER, P.LOAIPHONGBAN_ID
      FROM PhongbanRec AS PR
      INNER JOIN dbo.TM_PHONGBAN AS P ON P.CAPTREN_ID = PR.CATEGORY_ID AND P.BENHVIEN_ID = '$BENHVIEN_ID$' AND P.TAMNGUNG = '0'
      )
      SELECT '' AS TREE_COLUMN, P.* FROM PhongbanRec AS P
      INNER JOIN LST_DICTIONARY AS D on P.LOAIPHONGBAN_ID = D.DICTIONARY_ID AND D.DICTIONARY_TYPE_CODE = 'LoaiPhongBan'
      <dynamic prepend="where">
        <isParameterPresent>
          <isNotEmpty prepend="and" property="LOAIPHONGBAN_CODE">
            D.DICTIONARY_CODE IN (SELECT DICTIONARY_CODE FROM LST_DICTIONARY WHERE DICTIONARY_TYPE_CODE = 'LoaiPhongBan' and ENABLED = '1')
          </isNotEmpty>
        </isParameterPresent>
      </dynamic>
    </statement>
    <statement id="DAO00212_DEP_GetListByID" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
      SELECT * FROM TM_PHONGBAN_KHO WHERE PHONGBAN_ID = '$PHONGBAN_ID$' AND BENHVIEN_ID ='$BENHVIEN_ID$'
    </statement>
    <statement id="DAO00212_DEP_GetDetailByIDID" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
