    <statement id="DAO00019_Update_TT_TIEPNHAN_SOTHEBHYT_by_LastSOBHYT" parameterClass="DataObject" resultClass="DataObject">
      <isNotEmpty prepend="" property="DOITUONG_ID">

        UPDATE TT_TIEPNHAN_SOTHEBHYT
        set
        DOITUONG_ID = #DOITUONG_ID#,
        TYLE_VUOT_LCB = #TYLE_VUOT_LCB#,
        TYLE_TRAITUYEN = #TYLE_TRAITUYEN#,
        GIOIHAN_VUOT_LCB = #GIOIHAN_VUOT_LCB#,
        LUONG_COSO = #LUONG_COSO#,
        TYLE_THUOC_UT = #TYLE_THUOC_UT#,
        MADOITUONG = #MADOITUONG#
        where SOBHYT_HUONG_ID = #SOBHYT_HUONG_ID#

        UPDATE TT_TIEPNHAN
        set
        SOBHYT = #SOBHYT#
        , BHYTTUNGAY = #BHYTTUNGAY#
        , BHYTDENNGAY = #BHYTDENNGAY#
        , TRAITUYEN = '$TRAITUYEN$'
        , NGAYMIENDONGCHITRA = #NGAYMIENDONGCHITRA#
        , DOITUONG_ID = #DOITUONG_ID#
        where TIEPNHAN_ID = #TIEPNHAN_ID#
      </isNotEmpty>
    </statement>
    <statement id="DAO00019_Update_TT_TIEPNHAN_SOTHEBHYT" parameterClass="DataObject" resultClass="DataObject">
