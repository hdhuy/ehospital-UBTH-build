      , NGUON_BENHNHAN_ID
      , LOAITIEPNHAN_ID
      , THONGTIN_KCB_BHYT
      , NGAYTAO
      , NGUOITAO_ID
      , LYDODENKHAM
      <isPropertyAvailable prepend="," property="NGUOINUOCNGOAI">
        NGUOINUOCNGOAI
      </isPropertyAvailable>
      <isPropertyAvailable prepend="," property="ISTHONGTUYEN">
        ISTHONGTUYEN
      </isPropertyAvailable>
      )
      OUTPUT INSERTED.TIEPNHAN_ID, INSERTED.SOTIEPNHAN
      values (
      @Sotiepnhan
      , #UUTIEN#
      , #BENHNHAN_ID#
      , #BENHVIEN_ID#
      , #TUYENKHAMBENH_ID#
      , #NOITIEPNHAN_ID#
