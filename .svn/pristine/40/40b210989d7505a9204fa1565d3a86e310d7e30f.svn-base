      <isNotEmpty prepend="and" property="NGAYHETHAN">
        A.NGAYHETHAN &gt;= '$NGAYHIENTAI$'
      </isNotEmpty>
      and A.BENHVIEN_ID = '$BENHVIEN_ID$'
      order by THE_ID desc
