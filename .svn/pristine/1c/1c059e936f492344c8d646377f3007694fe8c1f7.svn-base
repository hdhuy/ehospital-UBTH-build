      <statement id="DAO00322_GetList" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
        select *
        from TT_MAU_CHUNGTU
        where BENHVIEN_ID = '$BENHVIEN_ID$'
        <isParameterPresent>
          <isNotEmpty prepend="and" property="CHUNGTU_ID">
