        <statement id="DAO00048_TT_PHAUTHUAT_GetDetail" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
            select PT.*, TN.BENHNHAN_ID
            from
            TT_PHAUTHUAT PT, TT_TIEPNHAN TN
