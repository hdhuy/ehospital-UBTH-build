      </isNotEmpty>
      ) PO ON PR.MACHUNGTUPR=PO.MACHUNGTUPR AND PR.CHUNGTUCHITIET_IDPR=PO.CHUNGTUCHITIET_IDPR
      where CTCT.BENHVIEN_ID = '$BENHVIEN_ID$' AND CTCT.CHUNGTU_ID = '$CHUNGTU_ID$'
