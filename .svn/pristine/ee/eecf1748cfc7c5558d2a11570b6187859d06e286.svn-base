    <statement id="DAO00400_PUR001_ChungTuSearchP01" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
      SELECT  0 AS CHON ,
      CT.CHUNGTU_ID ,
      CT.MACHUNGTU ,
      CT.NGAYLAPCHUNGTU ,
      CT.NGAYGIOLAPCHUNGTU ,
      CT.PROCESS_CODE ,
      CT.PROCESS_NAME ,
      CT.STATE_CODE ,
      CT.STATE_NAME ,
      CT.PHONGBAN_ID ,
      CT.TENPHONGBAN ,
      CT.NGUOIDUYETCAP1_ID ,
      CT.TENNGUOIDUYETCAP1 ,
      CT.NGAYGIODUYETCAP1 ,
      CT.NGUOIDUYETCAP2_ID ,
      CT.TENNGUOIDUYETCAP2 ,
      CT.NGAYGIODUYETCAP2 ,
      CT.NGUOIDUYETCAP3_ID ,
      CT.TENNGUOIDUYETCAP3 ,
      CT.NGAYGIODUYETCAP3 ,
      CT.CAPDUYET ,
      CT.THOIGIANGIAOHANG ,
      CT.NHACUNGCAP_ID ,
      CT.TENNHACUNGCAP ,
      CT.HINHTHUCGIAOHANG ,
      CT.HINHTHUCTHANHTOAN ,
      CT.TONGTIEN ,
      CT.GHICHU ,
      CT.DIENGIAI ,
      CT.LYDOTUCHOINHAN ,
      CT.TENNGUOITAO ,
      CT.TENNGUOICAPNHAT ,
      ISNULL(CT.NGUOIPHANCONG_ID,PC.NHANVIENPHANCONG_ID) AS NGUOIPHANCONG_ID,
      ISNULL(CT.TENNGUOIPHANCONG, '' )  AS TENNGUOIPHANCONG,
      ISNULL(CT.NGAYPHANCONG,ISNULL(CT.NGAYGIODUYETCAP3, ISNULL(CT.NGAYGIODUYETCAP2, CT.NGAYGIODUYETCAP1))) AS NGAYPHANCONG,
      CT.NGAYCHAPNHAN ,
      CT.NGAYTUCHOI ,
      CT.THANHTOAN1 ,
      CT.THANHTOAN2 ,
      CT.THANHTOAN3 ,
      CT.THANHTOAN4 ,
      CT.THANHTOAN5 ,
      CT.THANHTOAN6 ,
      CT.MALOAIPHIEU ,
      ( CASE WHEN CT.MALOAIPHIEU = 'DP' THEN N'Dược phẩm'
      ELSE CT.TENLOAIPHIEU
      END ) AS TENLOAIPHIEU ,
      CT.BENHVIEN_ID ,
      CT.NGAYTAO ,
      CT.NGUOITAO_ID ,
      CT.NGAYCAPNHAT ,
      CT.NGUOICAPNHAT_ID ,
      ( CASE WHEN CT.STATE_CODE = '3'
      THEN N'Đã duyệt bởi ' + ISNULL(CT.TENNGUOIDUYETCAP3, ISNULL(CT.TENNGUOIDUYETCAP2,CT.TENNGUOIDUYETCAP1))
      ELSE CASE WHEN CT.STATE_CODE = '4'
      THEN N'Đã từ chối bởi ' + ISNULL(CT.TENNGUOIDUYETCAP3, ISNULL(CT.TENNGUOIDUYETCAP2,CT.TENNGUOIDUYETCAP1))
      ELSE CT.STATE_NAME
      END
      END ) AS TRANGTHAI ,
      PO.MATONGHOP ,
      DT.MACHUNGTU AS MACHUNGTUDUOC,
      NGAYGIODUYETCAP = ISNULL(CT.NGAYGIODUYETCAP3, ISNULL(CT.NGAYGIODUYETCAP2, CT.NGAYGIODUYETCAP1))
      , CASE WHEN CT.MALOAIPHIEU != 'DP' THEN NULL ELSE KHO.TENKHO END AS KHODUOC
      , CASE WHEN CT.MALOAIPHIEU != 'DP' THEN CT.TENPHONGBAN ELSE PHONG.TENPHONGBAN END AS PHONGBAN
      FROM    TT_PUR_CHUNGTU CT
      LEFT JOIN TT_DUOC_DUTRU DT ON DT.MACHUNGTUMUAHANG = CT.MACHUNGTU
      LEFT JOIN TM_PHONGBAN_KHODUOC PK ON PK.KHODUOC_ID = DT.KHONHAP_ID
      LEFT JOIN TM_KHODUOC KHO ON KHO.KHODUOC_ID = PK.KHODUOC_ID
      LEFT JOIN TM_PHONGBAN PHONG ON PHONG.PHONGBAN_ID = PK.PHONGBAN_ID
      LEFT JOIN (
      SELECT DISTINCT YY.MATONGHOP
      FROM TT_PUR_CHUNGTU XX INNER JOIN TT_PUR_CHUNGTUCHITIET YY ON XX.CHUNGTU_ID=YY.CHUNGTU_ID
      WHERE XX.PROCESS_CODE='PO' AND XX.BENHVIEN_ID = '$BENHVIEN_ID$'
      <isNotEmpty prepend="AND" property="MACHUNGTU">
        YY.MATONGHOP = '$MACHUNGTU$'
      </isNotEmpty>
        ) PO ON CT.MACHUNGTU=PO.MATONGHOP
        LEFT JOIN
        (
        SELECT DISTINCT PCT.CHUNGTU_ID, PLOAI.NHANVIENPHANCONG_ID  FROM TT_PUR_CHUNGTUCHITIET PCT
          JOIN TM_PUR_LOAI PLOAI ON PCT.LOAI_ID = PLOAI.ID
          
        WHERE PCT.BENHVIEN_ID = '$BENHVIEN_ID$' AND ISNULL(PLOAI.NHANVIENPHANCONG_ID, 0) > 0
        )PC ON CT.CHUNGTU_ID = PC.CHUNGTU_ID

        where CT.BENHVIEN_ID = '$BENHVIEN_ID$'
      <isNotEmpty prepend="AND" property="PROCESS_CODE">
        CT.PROCESS_CODE='$PROCESS_CODE$'
      </isNotEmpty>
      <isNotEmpty prepend="AND" property="STATE_CODE">
        CT.STATE_CODE='$STATE_CODE$' AND CT.MACHUNGTUPOMOI IS NULL
      </isNotEmpty>
      <isNotEmpty prepend="AND" property="PHONGBAN_ID">
        CT.PHONGBAN_ID='$PHONGBAN_ID$'
      </isNotEmpty>
      <isNotEmpty prepend="AND" property="NGUOITAO_ID">
        CT.NGUOITAO_ID='$NGUOITAO_ID$'
      </isNotEmpty>
      <isNotEmpty prepend="AND" property="NGUOIPHANCONG_ID">
        ISNULL(CT.NGUOIPHANCONG_ID,PC.NHANVIENPHANCONG_ID) ='$NGUOIPHANCONG_ID$'
      </isNotEmpty>
      <isNotEmpty prepend="AND" property="MACHUNGTU">
        CT.MACHUNGTU LIKE '%' + #MACHUNGTU# + '%'
      </isNotEmpty>
      <isNotEmpty prepend="AND" property="TUNGAY">
        CONVERT(DATE,CT.NGAYLAPCHUNGTU) BETWEEN '$TUNGAY$' AND '$DENNGAY$'
      </isNotEmpty>
      <isNotEmpty prepend="AND" property="PHONGBAN_OPTION">
        (PHONG.PHONGBAN_ID=#PHONGBAN_OPTION# OR CT.PHONGBAN_ID = #PHONGBAN_OPTION#)
      </isNotEmpty>
      <isNotEmpty prepend="AND" property="KHODUOC_OPTION">
        KHO.KHODUOC_ID=#KHODUOC_OPTION#
      </isNotEmpty>
      ORDER BY NGAYLAPCHUNGTU
    </statement>
    <statement id="DAO00400_PUR001_ChungTuSearchP01_PO" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
      select 0 AS CHON,CT.CHUNGTU_ID,CT.MACHUNGTU,CT.NGAYLAPCHUNGTU,CT.NGAYGIOLAPCHUNGTU,CT.PROCESS_CODE,CT.PROCESS_NAME,CT.STATE_CODE,CT.STATE_NAME,CT.PHONGBAN_ID,
