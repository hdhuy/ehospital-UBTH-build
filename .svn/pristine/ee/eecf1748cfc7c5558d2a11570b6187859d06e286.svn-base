      G.SOHOPDONG, G.LOAIHOPDONG_ID, G.FILE_NAME, G.FULLPATHLOCAL, G.FULLPATHSERVER
      ,GC.SOLUONG, SUM(ISNULL(CCT1.SOLUONGYEUCAU, 0)) AS SOLUONGYEUCAU, (GC.SOLUONG - SUM(ISNULL(CCT1.SOLUONGYEUCAU, 0))) AS SOLUONGCONLAI
      FROM TM_PUR_GIAHOPDONG AS G
      INNER JOIN TM_PUR_GIAHOPDONGCHITIET AS GC ON GC.GIAHOPDONG_ID = G.GIAHOPDONG_ID AND GC.BENHVIEN_ID = G.BENHVIEN_ID
      LEFT JOIN TT_PUR_CHUNGTUCHITIET CCT ON CCT.GIAHOPDONG_ID = G.GIAHOPDONG_ID AND CCT.HANGHOA_ID = GC.HANGHOA_ID And CCT.STATE_NAME = 'DaLapPhieuPO'
	    LEFT JOIN TT_PUR_CHUNGTU CT ON CT.CHUNGTU_ID = CCT.CHUNGTU_ID AND CT.BENHVIEN_ID = G.BENHVIEN_ID
	    LEFT JOIN TT_PUR_CHUNGTUCHITIET CCT1 ON CCT1.HANGHOA_ID = CCT.HANGHOA_ID AND CCT1.MAHANGHOA = CCT.MAHANGHOA And CCT1.MATONGHOP = CT.MACHUNGTU
      AND CCT.MALOAIPHIEU = G.MALOAIPHIEU And CCT.BENHVIEN_ID = G.BENHVIEN_ID
      WHERE G.BENHVIEN_ID = '$BENHVIEN_ID$' AND G.GIAHOPDONG_ID = '$GIAHOPDONG_ID$'
      GROUP BY GC.GIAHOPDONGCHITIET_ID,G.GIAHOPDONG_ID, GC.HANGHOA_ID, GC.MAHANGHOA, GC.TENHANGHOA, GC.DONVITINH_ID, GC.TENDONVITINH, GC.DONGIA, GC.VAT, GC.GIASAUVAT, G.NHACUNGCAP_ID, G.TENNHACUNGCAP, G.NGAYHOPDONG,G.TUNGAY,G.DENNGAY,GC.KHONGDUNG,
      G.SOHOPDONG, G.LOAIHOPDONG_ID, G.FILE_NAME, G.FULLPATHLOCAL, G.FULLPATHSERVER, GC.SOLUONG
    </statement>
   
    <statement id="DAO00400_PUR001_AddGiaHopDong" parameterClass="System.Collections.IDictionary" resultClass="System.Int64">
      INSERT INTO TM_PUR_GIAHOPDONG(SOHOPDONG, LOAIHOPDONG_ID, NHACUNGCAP_ID,TENNHACUNGCAP,NGAYHOPDONG,TUNGAY,DENNGAY,MALOAIPHIEU,TENLOAIPHIEU,BENHVIEN_ID,NGAYTAO,NGUOITAO_ID,
      FILE_NAME,FULLPATHLOCAL,FULLPATHSERVER)
      VALUES(#SOHOPDONG#,#LOAIHOPDONG_ID#,#NHACUNGCAP_ID#,#TENNHACUNGCAP#,#NGAYHOPDONG#,#TUNGAY#,#DENNGAY#,#MALOAIPHIEU#,#TENLOAIPHIEU#,#BENHVIEN_ID#,GETDATE(),#NGUOITAO_ID#,
      #FILE_NAME#,#FULLPATHLOCAL#,#FULLPATHSERVER#)
      SELECT SCOPE_IDENTITY()
    </statement>
    <update id="DAO00400_PUR001_UpdGiaHopDong" parameterClass="DataObject">
      UPDATE TM_PUR_GIAHOPDONG SET SOHOPDONG = #SOHOPDONG#,LOAIHOPDONG_ID = #LOAIHOPDONG_ID#,NHACUNGCAP_ID = #NHACUNGCAP_ID#,TENNHACUNGCAP = #TENNHACUNGCAP#,NGAYHOPDONG = #NGAYHOPDONG#,TUNGAY = #TUNGAY#,DENNGAY = #DENNGAY#,MALOAIPHIEU = #MALOAIPHIEU#,TENLOAIPHIEU = #TENLOAIPHIEU#,NGAYCAPNHAT = GETDATE(),NGUOICAPNHAT_ID = #NGUOICAPNHAT_ID#
      ,FILE_NAME = #FILE_NAME#,FULLPATHLOCAL = #FULLPATHLOCAL#,FULLPATHSERVER = #FULLPATHSERVER#
      WHERE GIAHOPDONG_ID = '$GIAHOPDONG_ID$' AND BENHVIEN_ID = '$BENHVIEN_ID$'
    </update>
    <!--//NhanTT25 UPD END-->
    
    <statement id="DAO00400_PUR001_AddGiaHopDongChiTiet" parameterClass="System.Collections.IDictionary" resultClass="System.Int64">
      INSERT INTO TM_PUR_GIAHOPDONGCHITIET (GIAHOPDONG_ID,HANGHOA_ID,MAHANGHOA,TENHANGHOA,DONGIA,SOLUONG,DONVITINH_ID,TENDONVITINH,VAT,GIASAUVAT,KHONGDUNG,BENHVIEN_ID,NGAYTAO,NGUOITAO_ID)
      VALUES(#GIAHOPDONG_ID#,#HANGHOA_ID#,#MAHANGHOA#,#TENHANGHOA#,#DONGIA#,#SOLUONG#,#DONVITINH_ID#,#TENDONVITINH#,#VAT#,#GIASAUVAT#,#KHONGDUNG#,#BENHVIEN_ID#,GETDATE(),#NGUOITAO_ID#)
    </statement>
    <update id="DAO00400_PUR001_UpdGiaHopDongChiTiet" parameterClass="DataObject">
      UPDATE TM_PUR_GIAHOPDONGCHITIET SET GIAHOPDONG_ID = #GIAHOPDONG_ID#,HANGHOA_ID = #HANGHOA_ID#,MAHANGHOA = #MAHANGHOA#,TENHANGHOA = #TENHANGHOA#,DONGIA = #DONGIA#,SOLUONG = #SOLUONG#,DONVITINH_ID = #DONVITINH_ID#,TENDONVITINH = #TENDONVITINH#,VAT = #VAT#,GIASAUVAT = #GIASAUVAT#,KHONGDUNG = #KHONGDUNG#,NGAYCAPNHAT = GETDATE(),NGUOICAPNHAT_ID = #NGUOICAPNHAT_ID#
      WHERE GIAHOPDONGCHITIET_ID = '$GIAHOPDONGCHITIET_ID$' AND BENHVIEN_ID = '$BENHVIEN_ID$'
    </update>
    <statement id="DAO00400_PUR001_DelGiaHopDongChiTietTheoID" parameterClass="System.String" resultClass="System.Int32">
      delete from TM_PUR_GIAHOPDONGCHITIET where GIAHOPDONGCHITIET_ID = '$GIAHOPDONGCHITIET_ID$' AND BENHVIEN_ID = '$BENHVIEN_ID$'
    </statement>
    <statement id="DAO00400_PUR001_DelGiaHopDongAll" parameterClass="System.String" resultClass="System.Int32">
      delete from TM_PUR_GIAHOPDONGCHITIET where GIAHOPDONG_ID = '$GIAHOPDONG_ID$' AND BENHVIEN_ID = '$BENHVIEN_ID$'
      delete from TM_PUR_GIAHOPDONG where GIAHOPDONG_ID = '$GIAHOPDONG_ID$' AND BENHVIEN_ID = '$BENHVIEN_ID$'
    </statement>
    <statement id="DAO00400_PUR001_KTXoaGiaHopDong" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
      select TOP 1 CTCT.GIAHOPDONG_ID from TT_PUR_CHUNGTUCHITIET CTCT
      where CTCT.GIAHOPDONG_ID = '$GIAHOPDONG_ID$' AND CTCT.BENHVIEN_ID = '$BENHVIEN_ID$'
    </statement>
    <statement id="DAO00400_PUR001_GetGiaHopDong_HangHoaID" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
      SELECT DISTINCT TOP 1 G.GIAHOPDONG_ID,G.NHACUNGCAP_ID,G.TENNHACUNGCAP,G.NGAYHOPDONG,G.TUNGAY,G.DENNGAY,GC.DONGIA,GC.VAT,GC.GIAHOPDONGCHITIET_ID
      FROM TM_PUR_GIAHOPDONG G
      INNER JOIN TM_PUR_GIAHOPDONGCHITIET GC ON G.GIAHOPDONG_ID=GC.GIAHOPDONG_ID
      WHERE GC.HANGHOA_ID='$HANGHOA_ID$' And G.MALOAIPHIEU = '$MALOAIPHIEU$'
      AND ('$NGAYLAPCHUNGTU$' BETWEEN G.TUNGAY AND G.DENNGAY) AND G.BENHVIEN_ID='$BENHVIEN_ID$'
      ORDER BY GC.DONGIA ASC
    </statement>
    <statement id="DAO00400_PUR001_GetChungTuTheoHangHoaID" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
      SELECT CHUNGTU_ID, CHUNGTUCHITIET_ID, HANGHOA_ID FROM TT_PUR_CHUNGTUCHITIET WHERE HANGHOA_ID = '$HANGHOA_ID$' AND BENHVIEN_ID='$BENHVIEN_ID$'
      UNION ALL
      SELECT A.CHUNGTUNHAP_ID AS CHUNGTU_ID, A.CHUNGTUNHAPCHITIET_ID AS CHUNGTUCHITIET_ID, B.HANGHOA_ID FROM TT_INV_CHUNGTUNHAPCHITIET A LEFT JOIN TT_INV_THEODOI B ON B.THEODOI_ID = A.THEODOI_ID WHERE B.HANGHOA_ID = '$HANGHOA_ID$' AND A.BENHVIEN_ID='$BENHVIEN_ID$'
    </statement>
    <statement id="DAO00400_PUR001_GetCapDuyet" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
      select CT.CAPDUYET FROM TT_PUR_CHUNGTU CT
      where CT.BENHVIEN_ID = '$BENHVIEN_ID$' AND CT.MACHUNGTU = '$MACHUNGTU$'
    </statement>
    <statement id="DAO00400_GetPurWithID" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
      select *
      from TT_PUR_CHUNGTU
      where CHUNGTU_ID = '$CHUNGTU_ID$'
      <isNotEmpty prepend="and" property="STATE_CODE">
        STATE_CODE = '$STATE_CODE$'
      </isNotEmpty>
      <isNotEmpty prepend="and" property="PROCESS_CODE">
        PROCESS_CODE = '$PROCESS_CODE$'
      </isNotEmpty>
      <isNotEmpty prepend="and" property="BENHVIEN_ID">
        BENHVIEN_ID = '$BENHVIEN_ID$'
      </isNotEmpty>
    </statement>
    <!--Nhantt25 Add-->
    <statement id="DAO00400_PUR001_GetLoaiHopDong" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
      SELECT DICTIONARY_ID AS LOAIHOPDONG_ID, DICTIONARY_NAME AS LOAIHOPDONG_NAME FROM LST_DICTIONARY WHERE DICTIONARY_TYPE_CODE = 'LoaiHopDong'
    </statement>
        <statement id="DAO00400_PUR001_GetHangHoaValue" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
      SELECT VALUE FROM TM_SYS_APPSETTINGS WHERE CODE = 'PUR_CANHBAOHETHANGHOA'
    </statement>
    <statement id="DAO00400_PUR001_GIAHOPDONGCHITIET_CANHBAO" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
      SELECT G.TUNGAY,G.DENNGAY, GC.SOLUONG, SUM(ISNULL(CCT.SOLUONGYEUCAU, 0)) AS SOLUONGYEUCAU, (GC.SOLUONG - SUM(ISNULL(CCT.SOLUONGYEUCAU, 0))) AS SOLUONGCONLAI,
	          (SELECT VALUE FROM TM_SYS_APPSETTINGS WHERE CODE = 'PUR_CANHBAOHETHANGHOA') as SLCANHBAO,
            (SELECT VALUE FROM TM_SYS_APPSETTINGS WHERE CODE = 'PUR_CANHBAOHETHANHOPDONG') as DateCANHBAO
	    FROM TM_PUR_GIAHOPDONG AS G
      INNER JOIN TM_PUR_GIAHOPDONGCHITIET AS GC ON GC.GIAHOPDONG_ID = G.GIAHOPDONG_ID AND GC.BENHVIEN_ID = G.BENHVIEN_ID
	    LEFT JOIN TT_PUR_CHUNGTUCHITIET CCT ON CCT.GIAHOPDONG_ID = G.GIAHOPDONG_ID AND CCT.HANGHOA_ID = GC.HANGHOA_ID And CCT.BENHVIEN_ID = G.BENHVIEN_ID
      WHERE G.BENHVIEN_ID = '$BENHVIEN_ID$'  
	    AND G.GIAHOPDONG_ID = '$GIAHOPDONG_ID$'
	    AND GC.HANGHOA_ID = '$HANGHOA_ID$'
      GROUP BY G.TUNGAY,G.DENNGAY, GC.SOLUONG
    </statement>

    <!--Nhantt25 Add Dinh Kem List File Hop Dong-->
    <insert id="DAO00400_PUR025_AddListFile" parameterClass="DataObject" resultClass="System.Int64">
      INSERT INTO TM_PUR_DINHKEMHOPDONG
      ([GIAHOPDONG_ID],[NHACUNGCAP_ID]
      ,[ORIGINAL_NAME],[UPLOAD_NAME]
      ,[UPLOAD_FOLDER],[LOCAL_FOLDER]
      ,[TYPE],[SIZE]
      ,[BENHVIEN_ID],[NGAYTAO]
      ,[NGUOITAO_ID])
      VALUES
      (#GIAHOPDONG_ID#,#NHACUNGCAP_ID#
      ,#ORIGINAL_NAME#,#UPLOAD_NAME#
      ,#UPLOAD_FOLDER#,#LOCAL_FOLDER#
      ,#TYPE#,#SIZE#
      ,#BENHVIEN_ID#,#NGAYTAO#
      ,#NGUOITAO_ID#
      )
      SELECT SCOPE_IDENTITY() as value
    </insert>
    
    <update id="DAO00400_PUR025_UpdateListFile" parameterClass="DataObject">
      UPDATE TM_PUR_DINHKEMHOPDONG
      SET ORIGINAL_NAME = #ORIGINAL_NAME#, UPLOAD_NAME = #UPLOAD_NAME#,
      UPLOAD_FOLDER = #UPLOAD_FOLDER#, LOCAL_FOLDER = #LOCAL_FOLDER#,
      TYPE = #TYPE#, SIZE = #SIZE#, BENHVIEN_ID = #BENHVIEN_ID#
      where FILE_ID = '$FILE_ID$' AND GIAHOPDONG_ID = '$GIAHOPDONG_ID$' AND NHACUNGCAP_ID = '$NHACUNGCAP_ID$' AND BENHVIEN_ID = '$BENHVIEN_ID$'
    </update>
    
    <statement id="DAO00400_PUR025_DeleteListFileByID" parameterClass="System.String" resultClass="System.Int32">
      delete from TM_PUR_DINHKEMHOPDONG where FILE_ID = '$FILE_ID$'
    </statement>
    <statement id="DAO00400_PUR025_DeleteListFileByHopDongID" parameterClass="System.String" resultClass="System.Int32">
      delete from TM_PUR_DINHKEMHOPDONG where GIAHOPDONG_ID = '$GIAHOPDONG_ID$'
    </statement>
    <statement id="DAO00400_PUR025_GetFileByID" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
      select FILE_ID,GIAHOPDONG_ID,NHACUNGCAP_ID,ORIGINAL_NAME
      ,UPLOAD_NAME,UPLOAD_FOLDER,LOCAL_FOLDER,TYPE,SIZE,BENHVIEN_ID,NGAYTAO,NGUOITAO_ID
      From TM_PUR_DINHKEMHOPDONG where BENHVIEN_ID = '$BENHVIEN_ID$' And FILE_ID = '$FILE_ID$'
      Order by FILE_ID
    </statement>
    <statement id="DAO00400_PUR025_GetFileByHopDongID" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
      Select ROW_NUMBER() OVER(Order by FILE_ID) as STT, 
      FILE_ID,GIAHOPDONG_ID,NHACUNGCAP_ID,ORIGINAL_NAME,UPLOAD_NAME,UPLOAD_FOLDER,
      LOCAL_FOLDER,TYPE,SIZE,BENHVIEN_ID,NGAYTAO,NGUOITAO_ID
      From TM_PUR_DINHKEMHOPDONG where BENHVIEN_ID = '$BENHVIEN_ID$' And GIAHOPDONG_ID = '$GIAHOPDONG_ID$'
      Order by FILE_ID
    </statement>
    
    <statement id="DAO00400_PUR001_CheckMaHangHoa" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
      select * From TM_PUR_HANGHOA Where MA = '$MAHANGHOA$'
    </statement>
    

  </statements>
