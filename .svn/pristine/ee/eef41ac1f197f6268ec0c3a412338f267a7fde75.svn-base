            SELECT NHOMDICHVU_ID
            ,MANHOMDICHVU
            ,LOAIDICHVU_ID
            ,TENNHOMDICHVU
            ,CAP
            ,CAPTREN_ID
            ,TRALOIKETQUARIENG
            ,TAMNGUNG
            ,TENKHONGDAU
            ,NGAYTAO
            ,NGUOITAO_ID
            ,NGAYCAPNHAT
            ,NGUOICAPNHAT_ID
            FROM TM_NHOMDICHVU
            <!--where TAMNGUNG = 0 and BENHVIEN_ID = '$BENHVIEN_ID$'-->
            where BENHVIEN_ID = '$BENHVIEN_ID$'
            order by NHOMDICHVU_ID
