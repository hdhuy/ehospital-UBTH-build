      TIEPNHAN_ID = '$TIEPNHAN_ID$'
      and KHAMBENH_ID in ($LIST_KHAMBENH_ID$)
      and DUOC_ID is not null AND DUYET = '1'
      and HUY = '0'
    </statement>
    <!--Revision:0,aea037c0bb1bc032c8a9ce0d90dafc84-->
