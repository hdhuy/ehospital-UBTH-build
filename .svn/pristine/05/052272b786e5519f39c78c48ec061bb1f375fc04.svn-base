    <statement id="DAO00023_GetByTiepNhanID" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
      SELECT TIEPNHAN_ID,
      [LOAI_CHIPHI],
      [BENHAN_ID],
      [KHAMBENH_ID],
      [REF_ID],
      [REF_TABLE],
      [DUOC_ID],
      [BHTN_TL],
      [BENHNHAN_PHAITHANHTOAN],
      [BENHVIEN_ID],
      [BHTN_THANHTIEN_CHITRA],
      [BHYT_DONGIA],
      [BHYT_DONGIA_CHITRA],
      [BHYT_THANHTIEN],
      [BHYT_THANHTIEN_CHITRA],
      [BHYT_TL],
      [DICHVU_ID],
      [DONGIA_DOANHTHU],
      [GIATRI_BENHNHAN_DATHANHTOAN],
      [GIATRI_BIENLAI],
      [GIATRI_BIENLAI_DATHANHTOAN],
      [GIATRI_HOADON],
      [GIATRI_HOADON_DATHANHTOAN],
      [GIATRI_THATTHU],
      [HOADON_ID],
      [LOAIGIA_ID],
      [MIENGIAM_GIATRI],
      [MIENGIAM_TL],
      [NGANSACH_GIATRI],
      [NGANSACH_TL],
      [NGAYCAPNHAT],
      [NGAYTAO],
      [NGUOICAPNHAT_ID],
      [NGUOITAO_ID],
      [SOLUONG],
      [TAITRO_GIATRI],
      [TAITRO_TL],
      [THANHTIEN_DOANHTHU],
      [VIENPHI_ID],
      [TYLE_VAT],
