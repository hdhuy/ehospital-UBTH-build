            COMMIT TRANSACTION
            END TRY
            BEGIN CATCH
            IF @@TRANCOUNT &gt; 0
            ROLLBACK TRANSACTION
            SELECT -1 as result
            END CATCH
            END
            ELSE if @HU = '$SL$'
            SELECT 2 AS RESULT
            else
            select 0 as RESULT
        </statement>
