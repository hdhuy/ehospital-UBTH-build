                    </isNotEmpty>
                    <isNotEmpty prepend="and" property="BENHNHAN_PHAITHANHTOAN">
                        hdct.BENHNHAN_PHAITHANHTOAN = '$BENHNHAN_PHAITHANHTOAN$'
                    </isNotEmpty>
