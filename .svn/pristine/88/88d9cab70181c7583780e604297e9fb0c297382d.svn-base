                    </isNotEmpty>
                    <isNotEmpty prepend="and" property="BHYT_DONGIA">
                        hdct.BHYT_DONGIA = '$BHYT_DONGIA$'
                    </isNotEmpty>
                    <isNotEmpty prepend="and" property="BHYT_THANHTIEN">
                        hdct.BHYT_THANHTIEN = '$BHYT_THANHTIEN$'
