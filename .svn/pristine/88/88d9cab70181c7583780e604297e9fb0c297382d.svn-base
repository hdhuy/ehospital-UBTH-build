                    </isNotEmpty>
                    <isNotEmpty prepend="and" property="LOAI_CHIPHI">
                        hdct.LOAI_CHIPHI = '$LOAI_CHIPHI$'
                    </isNotEmpty>
                    <isNotEmpty prepend="and" property="LOAIGIA_ID">
                        hdct.LOAIGIA_ID = '$LOAIGIA_ID$'
                    </isNotEmpty>
