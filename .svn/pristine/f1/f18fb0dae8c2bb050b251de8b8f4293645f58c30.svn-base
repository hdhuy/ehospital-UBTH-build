      B.*
      from TT_TIEPNHAN A
      inner join TT_BENHNHAN B on B.BENHNHAN_ID = A.BENHNHAN_ID and (A.TRANGTHAI = 'DATIEPNHAN') and B.ACTIVE = '1'
      left join TT_NOITRU_BENHAN C on C.TIEPNHAN_ID = A.TIEPNHAN_ID
      where A.BENHVIEN_ID = '$BENHVIEN_ID$'
    </statement>
