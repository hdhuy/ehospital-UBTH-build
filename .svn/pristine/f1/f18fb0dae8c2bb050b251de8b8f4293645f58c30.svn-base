      where BENHVIEN_ID = '$BENHVIEN_ID$' group by KHODUOC_ID, DUOC_ID, NGUONDUOC_ID) A
      group by LTRIM(A.KHODUOC_ID) + '_' + LTRIM(A.DUOC_ID) + '_' + LTRIM(A.NGUONDUOC_ID), A.KHODUOC_ID, A.DUOC_ID, A.NGUONDUOC_ID)
