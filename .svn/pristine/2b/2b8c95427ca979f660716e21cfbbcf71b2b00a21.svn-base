        <statement id="DAO00019_GetTiepNhanGanNhat" resultClass="DataObject">
            select B.BENHNHAN_ID, isnull(convert(varchar, B.NGAYSINH, 103), convert(varchar, B.NAMSINH)) as NGAYTHANGNAMSINH
            ,T.TIEPNHAN_ID AS TIEPNHAN_ID_L, T.SOBHYT AS SOTHE_L, T.BHYTTUNGAY AS NGAYHIEULUC_L, T.BHYTDENNGAY AS NGAYHETHIEULUC_L, T.KHUVUCSONG_ID AS KHUVUCSONG_ID_L
            , T.NGAYMIENDONGCHITRA AS NGAYMIENDONGCHITRA_L, T.BHYTTREN5NAM AS TREN5NAM_L, T.TRAITUYEN AS TRAITUYEN_L
            from TT_BENHNHAN  B
            left join TT_TIEPNHAN  T ON T.TIEPNHAN_ID =
            (SELECT TOP 1 TIEPNHAN_ID FROM TT_TIEPNHAN WHERE BENHNHAN_ID = B.BENHNHAN_ID ORDER BY THOIGIANTIEPNHAN DESC)
            <dynamic prepend="where">
                <isParameterPresent>
                    <isNotEmpty prepend="and" property="BENHNHAN_ID">
                        B.BENHNHAN_ID = '$BENHNHAN_ID$'
                    </isNotEmpty>
                    <isNotEmpty prepend="and" property="MAYTE">
                        B.MAYTE = '$MAYTE$'
                    </isNotEmpty>
                </isParameterPresent>
            </dynamic>
            order by BENHNHAN_ID
        </statement>
      <update id="DAO00019_Update_TiepNhan_ThongtinChuyenVien" parameterClass="HashTable">
        update TT_TIEPNHAN set
        CHANDOANNGT = #CHANDOANNGT#,
        ICDNOIGIOITHIEU_ID = #ICDNOIGIOITHIEU_ID#,
        LYDOCHUYENVIEN_ID = #LYDOCHUYENVIEN_ID#,
        NOIGIOITHIEU_ID = #NOIGIOITHIEU_ID#,
        HINHTHUCCHUYENVIEN_ID = #HINHTHUCCHUYENVIEN_ID#,
        THOIGIANCHUYENTUYEN = #THOIGIANCHUYENTUYEN#,
        SOCHUYENVIEN = #SOCHUYENVIEN#
        where TIEPNHAN_ID = '$TIEPNHAN_ID$'
      </update>
      <statement id="DAO00019_Get_TiepNhan_ThongtinChuyenVien" resultClass="DataObject">
        select CHANDOANNGT, ICDNOIGIOITHIEU_ID, LYDOCHUYENVIEN_ID, NOIGIOITHIEU_ID, HINHTHUCCHUYENVIEN_ID, THOIGIANCHUYENTUYEN, SOCHUYENVIEN
        from TT_TIEPNHAN
        where TIEPNHAN_ID = '$TIEPNHAN_ID$'
      </statement>
      <statement id="DAO00019_Get_DVDelete_HasThanhToan" resultClass="DataObject">
        select count(*) TOTAL
        from TT_VIENPHI
        where TIEPNHAN_ID = '$TIEPNHAN_ID$' and REF_ID = '$DVYEUCAU_ID$'
        and REF_TABLE = 'TT_DVYEUCAU' and DICHVU_ID = '$DICHVU_ID$'
        and (HOADON_ID is not null or len(HOADON_ID) > 0)
      </statement>
      <statement id="DAO00019_Get_DVDelete_HasThucHien" resultClass="DataObject">
        select count(*) TOTAL
        from TT_DVYEUCAU
        where TIEPNHAN_ID = '$TIEPNHAN_ID$' and DVYEUCAU_ID = '$DVYEUCAU_ID$'
        and TRANGTHAI != 'CHUAKETQUA'
      </statement>
      <statement id="DAO00019_GetDanhsachKhambenh" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
        select *
        from
        (SELECT COUNT(*) OVER() as TOTALROWS, ROW_NUMBER() OVER(order by isnull($SOHANGDOI$,'999999999'), dv.NGAYGIOYEUCAU) as ROWNUMBER,
        bn.MAYTE, bn.TENBENHNHAN, bn.NAMSINH, bn.NGAYSINH as TUOI, ISNULL(CONVERT(VARCHAR(10), bn.NGAYSINH, 103),CAST(bn.NAMSINH AS varchar)) as NGAYSINH, bn.DIACHILIENLAC as DIACHI, bn.GIOITINH, GD.DESCRIPTION as TENGIOITINH,
        tn.DOITUONG_ID, tn.THOIGIANTIEPNHAN, tn.BENHNHAN_ID, tn.TIEPNHAN_ID, tn.TRAITUYEN, tn.SOTIEPNHAN,
        dv.DICHVU_ID, dv.NOITHUCHIEN_ID, isnull(dv.NGAYTAO, dv.NGAYCAPNHAT) as THOIGIANDKDICHVU,
        dv.KHAN, dv.NOIYEUCAU_ID, dv.NGAYGIOYEUCAU, dv.BACSICHIDINH_ID, dv.DONVIGIOITHIEU_ID, dv.NGUOIGIOITHIEU_ID,
        tn.NOIGIOITHIEU_ID, kbnt.HUONGGIAIQUYET_ID,
        dv.DVYEUCAU_ID, dv.MARIS, dv.TRANGTHAI as TRANGTHAICODE, B.TENNHOMDICHVU, dv.SOPHIEUYEUCAU, dv.KHAMBENH_NGOAITRU_ID, dv.NGUOICAPNHAT_ID, dv.NGAYCAPNHAT,
        CASE WHEN Left(B.MANHOMDICHVU,2) = '03' THEN '02' ELSE Left(B.MANHOMDICHVU,2) END AS MANHOMDICHVU, B.MANHOMDICHVU as MANHOMDICHVUCHITIET, tn.TRANGTHAI as TRANGTHAI_TIEPNHAN,
        '$DAKHAM$' as DAKHAM, isnull($SOHANGDOI$,'999999999') as SOHANGDOI,
        vp.DATHANHTOAN, dv.TRANGTHAI_WFL,
        BA.SOBENHAN, dv.CHANDOAN, dt.TENDOITUONG, isnull(kbnt.THOIGIANKHAM, dv.NGAYCAPNHAT) as THOIGIANKHAM,
        tn.SOBHYT
        FROM [TT_TIEPNHAN] tn
        join TT_BENHNHAN bn on tn.BENHNHAN_ID = bn.BENHNHAN_ID
        inner join TT_DVYEUCAU dv on dv.TIEPNHAN_ID = tn.TIEPNHAN_ID
        inner join TM_DICHVU A on A.DICHVU_ID = dv.DICHVU_ID
        inner join TM_NHOMDICHVU B on B.NHOMDICHVU_ID = A.NHOMDICHVU_ID
        left join TT_VIENPHI VP on VP.TIEPNHAN_ID = tn.TIEPNHAN_ID and VP.DICHVU_ID = dv.DICHVU_ID and VP.REF_ID = dv.DVYEUCAU_ID
        left join TT_NOITRU_BENHAN BA on BA.TIEPNHAN_ID = tn.TIEPNHAN_ID AND dv.BENHAN_ID = BA.BENHAN_ID
        inner join TM_GENDER GD on GD.ID = bn.GIOITINH
        left join TT_NGOAITRU_KHAMBENH kbnt on kbnt.KHAMBENH_ID = dv.KHAMBENH_NGOAITRU_ID
        left join TM_DOITUONG dt on dt.DOITUONG_ID = tn.DOITUONG_ID
        where
        dv.DICHVU_ID IS NOT NULL
        and tn.BENHVIEN_ID = '$BENHVIEN_ID$'
        and HUYYEUCAU = 0
        and dv.NGAYHENCLS is NULL
        <isNotEmpty prepend="and" property="TUNGAYTIEPNHAN">
          cast(dv.NGAYYEUCAU as date) &gt;= cast('$TUNGAYTIEPNHAN$' as date)
        </isNotEmpty>
        <isNotEmpty prepend="and" property="DENNGAYTIEPNHAN">
          cast(dv.NGAYYEUCAU as date) &lt;= cast('$DENNGAYTIEPNHAN$' as date)
        </isNotEmpty>
        <isNotEmpty prepend="and" property="NOITHUCHIEN_ID">
          dv.NOITHUCHIEN_ID = '$NOITHUCHIEN_ID$'
        </isNotEmpty>             
        <isNotEmpty prepend="" property="DAKHAM">
          <isEqual prepend="AND" property="DAKHAM" compareValue="True">
            (dv.TRANGTHAI_WFL not in ('CHODONGTIEN', 'CHUAKETQUA') or (dv.TRANGTHAI = 'HOANTAT' or dv.TRANGTHAI = 'DACHUYENTUYEN'  or dv.TRANGTHAI = 'CHOXACNHAN' OR dv.TRANGTHAI = 'DANGTHUCHIEN'))
            and (dv.TRANGTHAI != 'CHUAKETQUA' or dv.TRANGTHAI_WFL != 'HOANTHANH')
          </isEqual>
          <isEqual prepend="AND" property="DAKHAM" compareValue="False">
            ((dv.TRANGTHAI_WFL = 'CHUAKETQUA' or (dv.TRANGTHAI = 'CHUAKETQUA' and dv.TRANGTHAI_WFL = 'HOANTHANH')) and (kbnt.HUONGGIAIQUYET_ID is null or kbnt.HUONGGIAIQUYET_ID = 0))
            and (tn.TRANGTHAI != 'HOANTHANH')
          </isEqual>
        </isNotEmpty>          
        <isNotEmpty prepend="and" property="TENBENHNHAN">
          (bn.TENBENHNHAN like '%' + #TENBENHNHAN# + '%' or bn.MAYTE like '%' + #TENBENHNHAN# + '%')
        </isNotEmpty>
        <isNotEmpty prepend="and" property="MANHOMDICHVU">
          (CASE WHEN Left(B.MANHOMDICHVU,2) = '03' THEN '02' ELSE Left(B.MANHOMDICHVU,2) END) = '$MANHOMDICHVU$'
        </isNotEmpty>
        <isNotEmpty prepend="and" property="MANHOMDICHVU_EXCEPT">
          (CASE WHEN Left(B.MANHOMDICHVU,2) = '03' THEN '02' ELSE Left(B.MANHOMDICHVU,2) END) not in ($MANHOMDICHVU_EXCEPT$)
        </isNotEmpty>       
        ) lst
        <isNotEmpty prepend="" property="FROMROW">
          where
          lst.ROWNUMBER between $FROMROW$ and $TOROW$
        </isNotEmpty>
        order by SOHANGDOI, NGAYGIOYEUCAU asc
      </statement>
      <statement id="DAO00019_GetDanhsachTrangThaiKhamBenh" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
        select *            from
        (SELECT COUNT(*) OVER() as TOTALROWS, ROW_NUMBER() <!--OVER(order by isnull(dv.SOTHUTU,'999999999'), dv.NGAYGIOYEUCAU)-->
        OVER(order by bn.MAYTE, tn.sotiepnhan)
        as
        ROWNUMBER,
        bn.MAYTE, bn.TENBENHNHAN, bn.NAMSINH, bn.NGAYSINH as TUOI, ISNULL(CONVERT(VARCHAR(10), bn.NGAYSINH, 103),CAST(bn.NAMSINH AS varchar)) as NGAYSINH, bn.DIACHILIENLAC as
        DIACHI, bn.GIOITINH, GD.DESCRIPTION as TENGIOITINH,
        tn.DOITUONG_ID, tn.THOIGIANTIEPNHAN, tn.BENHNHAN_ID, tn.TIEPNHAN_ID, tn.TRAITUYEN, tn.SOTIEPNHAN,
        dv.DICHVU_ID,
        dv.NOITHUCHIEN_ID, isnull(dv.NGAYTAO, dv.NGAYCAPNHAT) as THOIGIANDKDICHVU,
        dv.KHAN, dv.NOIYEUCAU_ID, dv.NGAYGIOYEUCAU, dv.BACSICHIDINH_ID, dv.DONVIGIOITHIEU_ID,
        dv.NGUOIGIOITHIEU_ID,
        tn.NOIGIOITHIEU_ID, kbnt.HUONGGIAIQUYET_ID,
        dv.DVYEUCAU_ID, dv.MARIS, dv.TRANGTHAI as TRANGTHAICODE, B.TENNHOMDICHVU, dv.SOPHIEUYEUCAU,
        dv.KHAMBENH_NGOAITRU_ID, dv.NGUOICAPNHAT_ID, dv.NGAYCAPNHAT,
        CASE WHEN Left(B.MANHOMDICHVU,2) = '03' THEN '02' ELSE Left(B.MANHOMDICHVU,2) END AS MANHOMDICHVU, B.MANHOMDICHVU as MANHOMDICHVUCHITIET, tn.TRANGTHAI as TRANGTHAI_TIEPNHAN,
        '' as DAKHAM, isnull(dv.SOTHUTU,'999999999') as SOHANGDOI,
        vp.DATHANHTOAN,
        BA.SOBENHAN, dv.CHANDOAN
        FROM [TT_TIEPNHAN] tn
        join TT_BENHNHAN bn on tn.BENHNHAN_ID = bn.BENHNHAN_ID
        left join TT_DVYEUCAU dv on dv.TIEPNHAN_ID = tn.TIEPNHAN_ID
        left join TM_DICHVU A on A.DICHVU_ID = dv.DICHVU_ID
        left join TM_NHOMDICHVU B on B.NHOMDICHVU_ID = A.NHOMDICHVU_ID
        left join TT_VIENPHI VP on VP.TIEPNHAN_ID = tn.TIEPNHAN_ID and VP.DICHVU_ID = dv.DICHVU_ID and VP.REF_ID = dv.DVYEUCAU_ID
        left join TT_NOITRU_BENHAN BA on BA.TIEPNHAN_ID = tn.TIEPNHAN_ID AND dv.BENHAN_ID = BA.BENHAN_ID
        left join TM_GENDER GD on GD.ID = bn.GIOITINH
        left join TT_NGOAITRU_KHAMBENH kbnt on kbnt.KHAMBENH_ID = dv.KHAMBENH_NGOAITRU_ID
        where
        dv.DICHVU_ID IS NOT NULL
        and tn.BENHVIEN_ID = '$BENHVIEN_ID$'
        and HUYYEUCAU = 0
        and dv.NGAYHENCLS is NULL
        <isNotEmpty prepend="and" property="TUNGAY">
          tn.NGAYTIEPNHAN &gt;= '$TUNGAY$'
        </isNotEmpty>
        <isNotEmpty prepend="and" property="DENNGAY">
          tn.NGAYTIEPNHAN &lt;= '$DENNGAY$'
        </isNotEmpty>
        <isNotEmpty prepend="and" property="NOITHUCHIEN_ID">
            dv.NOITHUCHIEN_ID = '$NOITHUCHIEN_ID$'
        </isNotEmpty>
        <isNotEmpty prepend="and" property="TENBENHNHAN">
          (bn.TENBENHNHAN like '%' + #TENBENHNHAN# + '%' or bn.MAYTE like '%' + #TENBENHNHAN# + '%')
        </isNotEmpty>
        ) lst
        <isNotEmpty prepend="" property="FROMROW">
        where
        lst.ROWNUMBER between $FROMROW$ and $TOROW$
        </isNotEmpty>
        order by <!--SOHANGDOI, NGAYGIOYEUCAU-->MAYTE asc
      </statement>
      <statement id="DAO00019_GetBenhAnWithTiepNhan" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
        select * from TT_NOITRU_BENHAN where TIEPNHAN_ID = '$TIEPNHAN_ID$' and BENHVIEN_ID = '$BENHVIEN_ID$'
      </statement>
      <statement id="DAO00019_KiemtrabenhnhanNgoaitruChuaKHoabenhan" resultClass="DataObject">
        select A.* , C.TRANGTHAI as STATUS, C.SOTIEPNHAN, C.SOBHYT, C.BHYTTUNGAY, C.BHYTDENNGAY, C.NGAYTIEPNHAN
        from TT_BENHNHAN A
        inner join TT_TIEPNHAN C on C.BENHNHAN_ID = A.BENHNHAN_ID
        where (C.TRANGTHAI = 'DATIEPNHAN') and A.BENHVIEN_ID = '$BENHVIEN_ID$'  and A.ACTIVE = '1' and A.MAYTE = '$MAYTE$'
        <isNotEmpty prepend="and" property="TIEPNHAN_ID">
          C.TIEPNHAN_ID = '$TIEPNHAN_ID$'
        </isNotEmpty>
      </statement>
      <statement id="DAO00019_Add_TT_TIEPNHAN_SOTHEBHYT" parameterClass="DataObject" resultClass="DataObject">
        INSERT INTO TT_TIEPNHAN_SOTHEBHYT
        (TIEPNHAN_ID
        ,BENHNHAN_ID
        ,BENHAN_ID
        ,DOITUONG_ID
        ,SOBHYT
        ,BHYTTUNGAY
        ,BHYTDENNGAY
        ,BHYTNGAYCAP
        ,BHYTTREN3NAM
        ,BHYTTREN5NAM
        ,NGAYMIENDONGCHITRA
        ,TRAITUYEN
        ,BENHVIEN_ID
        ,NOITAO_ID
        ,NGAYTAO
        ,NGUOITAO_ID
        ,NGAYCAPNHAT
        ,NGUOICAPNHAT_ID)
        VALUES
        (#TIEPNHAN_ID#
        ,#BENHNHAN_ID#
        ,#BENHAN_ID#
        ,#DOITUONG_ID#
        ,#SOBHYT#
        ,#BHYTTUNGAY#
        ,#BHYTDENNGAY#
        ,#BHYTNGAYCAP#
        ,#BHYTTREN3NAM#
        ,#BHYTTREN5NAM#
        ,#NGAYMIENDONGCHITRA#
        ,#TRAITUYEN#
        ,#BENHVIEN_ID#
        ,#NOITAO_ID#
        ,#NGAYTAO#
        ,#NGUOITAO_ID#
        ,#NGAYCAPNHAT#
        ,#NGUOICAPNHAT_ID#)
      </statement>
      <statement id="DAO00019_Update_TT_TIEPNHAN_SOTHEBHYT" parameterClass="DataObject" resultClass="DataObject">
        update TT_TIEPNHAN_SOTHEBHYT
        set BENHAN_ID = '$BENHAN_ID$'
        where TIEPNHAN_ID = (select top 1 TIEPNHAN_ID from TT_TIEPNHAN_SOTHEBHYT where TIEPNHAN_ID = '$TIEPNHAN_ID$' order by SOBHYT_HUONG_ID desc)
      </statement>
      <statement id="DAO00019_GetLoaiGiaLoaiKhachHang" resultClass="DataObject">
        select A.*, B.DICTIONARY_CODE MAKHACHHANG, C.TENLOAIGIA
        from TM_NGUONKHACHHANG_LOAIGIA A
        left join LST_DICTIONARY B on A.NGUONKHACHHANG_BENHNHAN_ID = B.DICTIONARY_ID
        inner join TM_LOAIGIA C on C.LOAIGIA_ID = A.LOAIGIA_ID and CAPTREN_ID  = (select LOAIGIA_ID from TM_LOAIGIA where MALOAIGIA = 'GiaDichVu')
        where B.DICTIONARY_TYPE_CODE = 'NguonKhachHang'
      </statement>
      <statement id="DAO00019_GetLoaiGiaDuocLoaiKhachHang" resultClass="DataObject">
        select A.LOAIGIA_ID
        from TM_NGUONKHACHHANG_LOAIGIA A
        left join LST_DICTIONARY B on A.NGUONKHACHHANG_BENHNHAN_ID = B.DICTIONARY_ID
        inner join TM_LOAIGIA C on C.LOAIGIA_ID = A.LOAIGIA_ID and CAPTREN_ID  = (select LOAIGIA_ID from TM_LOAIGIA where MALOAIGIA = 'Giathuoc')
        where B.DICTIONARY_TYPE_CODE = 'NguonKhachHang' and B.DICTIONARY_CODE = '$MAKHACHHANG$'
      </statement>
      <statement id="DAO00019_GetListPhongKham_REG13" resultClass="DataObject">
        select pb.PHONGBAN_ID, pb.TENPHONGBAN, case when dv.ATTRIBUTE5 is null then 'TT' else 'KTC' end as LOAI_CHIPHI
        from TM_PHONGBAN pb
        left join TM_PHONGBAN_DICHVU pb_dv on pb_dv.PHONGBAN_ID = pb.PHONGBAN_ID and pb_dv.BENHVIEN_ID = pb.BENHVIEN_ID
        left join TM_DICHVU dv on dv.DICHVU_ID = pb_dv.DICHVU_ID and dv.BENHVIEN_ID = pb_dv.BENHVIEN_ID
        left join TM_NHOMDICHVU ndv on ndv.NHOMDICHVU_ID = dv.NHOMDICHVU_ID and ndv.BENHVIEN_ID = dv.BENHVIEN_ID
        where pb.BENHVIEN_ID = '$BENHVIEN_ID$' and ndv.MANHOMDICHVU = '04' and dv.DICHVU_ID IS NOT NULL
      </statement>
      <statement id="DAO00019_LayThongTinSoluongDichVuPhongKham_REG13" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
        select PHONGBAN_ID, TENPHONGBAN, NOITHUCHIEN_ID, SUM(CHUAKETQUA) AS CHUAKETQUA, SUM(HOANTAT) AS HOANTAT, SUM(DANGTHUCHIEN) AS DANGTHUCHIEN
        from (
        select B.PHONGBAN_ID, B.TENPHONGBAN, A.NOITHUCHIEN_ID,
        case when A.TRANGTHAI = 'CHUAKETQUA' then 1 else 0 end AS CHUAKETQUA,
        case when A.TRANGTHAI = 'HOANTAT' then 1 else 0 end AS HOANTAT,
        case when A.TRANGTHAI = 'DANGTHUCHIEN' then 1 else 0 end AS DANGTHUCHIEN
        from TM_PHONGBAN B
        left join TT_DVYEUCAU A on A.NOITHUCHIEN_ID = B.PHONGBAN_ID and A.BENHVIEN_ID = B.BENHVIEN_ID and A.NGAYYEUCAU = CONVERT(date, getdate())
        left join TM_DICHVU dv on A.DICHVU_ID = dv.DICHVU_ID
        left join TM_NHOMDICHVU ndv on ndv.NHOMDICHVU_ID = dv.NHOMDICHVU_ID
        where B.BENHVIEN_ID ='$BENHVIEN_ID$' and Left(ndv.MANHOMDICHVU,2) = '04'
        and B.PHONGBAN_ID in ($PHONGBAN_LIST$)
        ) tmp
        group by tmp.NOITHUCHIEN_ID, tmp.TENPHONGBAN, tmp.PHONGBAN_ID
      </statement>
      <statement id="DAO00019_GetDichVu" resultClass="DataObject">
        select * from TT_DVYEUCAU WHERE TIEPNHAN_ID = '$TIEPNHAN_ID$' and HUYYEUCAU = '0'
      </statement>
      <statement id="DAO00019_GetThongTinTiepNhanByKey" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
        select top 1 A.*, B.MAYTE, B.TENBENHNHAN, B.NAMSINH, B.NGAYSINH
        from TT_TIEPNHAN A
        inner join TT_BENHNHAN B on B.BENHNHAN_ID = A.BENHNHAN_ID and B.BENHVIEN_ID = A.BENHVIEN_ID
        where A.TIEPNHAN_ID = $TIEPNHAN_ID$
        and A.BENHVIEN_ID = '$BENHVIEN_ID$'
      </statement>
      <statement id="DAO00019_UpdateLoaiTiepNhan" parameterClass="System.Collections.IDictionary" resultClass="System.Int32">
        update TT_TIEPNHAN
        set
        LOAITIEPNHAN_ID = #LOAITIEPNHAN_ID#,
        NGUOICAPNHAT_ID = #NGUOICAPNHAT_ID#,
        NGAYCAPNHAT = #NGAYCAPNHAT#
        where TIEPNHAN_ID = $TIEPNHAN_ID$

        select #TIEPNHAN_ID#

      </statement>
      <statement id="DAO00019_LayDichVuTuDVYEUCAU" parameterClass="System.Collections.IDictionary" resultClass="System.Int32">
        select DICHVU_ID from TT_DVYEUCAU where DVYEUCAU_ID = #DVYEUCAU_ID#
      </statement>
      <statement id="DAO00019_UpdateDichVuPTTTMoi" parameterClass="System.Collections.IDictionary" resultClass="System.Int32">
        
        begin
        <!--dvyeucau-->
        update TT_DVYEUCAU set DICHVU_ID = #DICHVU_ID_2# where DVYEUCAU_ID = #DVYEUCAU_ID# and DICHVU_ID = #DICHVU_ID_1#
        <!--vienphi-->
        update TT_VIENPHI set DICHVU_ID = #DICHVU_ID_2#, DONGIA_DOANHTHU = #DONGIA_DOANHTHU_2#, BHYT_DONGIA = #BHYT_DONGIA_2#
        where REF_TABLE = 'TT_DVYEUCAU' and REF_ID = #DVYEUCAU_ID# and DICHVU_ID = #DICHVU_ID_1#
        <!--phau thuat yc-->
        update TT_PHAUTHUAT_YEUCAU set DICHVU_ID = #DICHVU_ID_2# where DVYEUCAU_ID = #DVYEUCAU_ID# and DICHVU_ID = #DICHVU_ID_1#
        end
      </statement>
    </statements>
</sqlMap>
