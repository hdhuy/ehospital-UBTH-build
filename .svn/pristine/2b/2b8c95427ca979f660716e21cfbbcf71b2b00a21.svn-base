              </isNotEmpty>
              <isNotEmpty prepend="and" property="DENNGAY">
                tn.NGAYTIEPNHAN &lt;= '$DENNGAY$'
              </isNotEmpty>
            </isEqual>
          </isNotEmpty>
            <isNotEmpty prepend="and" property="TENBENHNHAN">
              (tn.TENBENHNHAN like '%' + #TENBENHNHAN# + '%' or tn.MAYTE like '%' + #TENBENHNHAN# + '%')
            </isNotEmpty>
            
        </statement>
