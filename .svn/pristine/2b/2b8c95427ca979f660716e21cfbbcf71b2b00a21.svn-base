            </isNotEmpty>
            <isNotEmpty prepend="and" property="MANHOMDICHVU_EXCEPT">
                (CASE WHEN Left(B.MANHOMDICHVU,2) = '03' THEN '02' ELSE Left(B.MANHOMDICHVU,2) END) not in ($MANHOMDICHVU_EXCEPT$)
            </isNotEmpty>
          <isNotEmpty prepend="and" property="NOT_MANHOMDICHVU">
