            <isNotEmpty prepend="and" property="DENNGAY">
              E.NGAYYEUCAU &lt;= '$DENNGAY$'
            </isNotEmpty>
          </isEqual>
          <isEqual prepend="" property="YEUCAU" compareValue="0">
            <isNotEmpty prepend="and" property="TUNGAY">
              tn.NGAYTIEPNHAN &gt;= '$TUNGAY$'
            </isNotEmpty>
            <isNotEmpty prepend="and" property="DENNGAY">
              tn.NGAYTIEPNHAN &lt;= '$DENNGAY$'
            </isNotEmpty>
          </isEqual>
        </isNotEmpty>
        <isNotEmpty prepend="and" property="TENBENHNHAN">
          (tn.TENBENHNHAN like '%' + #TENBENHNHAN# + '%' or tn.MAYTE like '%' + #TENBENHNHAN# + '%')
        </isNotEmpty>
      </statement>
