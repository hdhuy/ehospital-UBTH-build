          left join TM_DUOC D on D.DUOC_ID = CLS_VT.DUOC_ID
          left join TM_LOAIDUOC LD on LD.LOAIDUOC_ID = D.LOAIDUOC_ID
          left join TT_DUOC_PHIEULINH PL on PL.CHUNGTU_ID = CLS_VT.CHUNGTU_ID
