          )json
          WHERE TT_DUOC_TONKHO_BOOK_NGOAITRU.REF_TABLE = json.REF_TABLE AND TT_DUOC_TONKHO_BOOK_NGOAITRU.REF_ID = json.REF_ID and TT_DUOC_TONKHO_BOOK_NGOAITRU.SOLUONG = 0
