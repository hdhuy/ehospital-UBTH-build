          N'Người bệnh: ' + BN.MAYTE + ' - ' + BN.TENBENHNHAN + ' - '  + N'Số TN: ' + TN.SOTIEPNHAN  as BENHNHAN,
          CASE WHEN BN.GIOITINH = 1 THEN 'Nam' WHEN BN.GIOITINH = 2 THEN N'Nữ' END  AS GIOITINH,
