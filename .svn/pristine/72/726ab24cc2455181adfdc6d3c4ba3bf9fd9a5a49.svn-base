            </isNotEmpty>
            <isNotEmpty prepend="and" property="TRANGTHAI_DAXUAT">
                TT.TRANGTHAI	!= 'CHUAXUAT'
            </isNotEmpty>
            AND KB.RAVIEN = '1'
            AND CONVERT(date, TT.NGAYTAO) BETWEEN CONVERT(date, '$TUNGAY$')  AND  CONVERT(date, '$DENNGAY$')
