            AND (BN.TENBENHNHAN like N'%$TUKHOA$%' OR BN.MAYTE like N'%$TUKHOA$%' OR  TN.SOTIEPNHAN  like N'%$TUKHOA$%' or D.TENDUOCDAYDU like N'%$TUKHOA$%'
