          UPDATE TT_DUOC_TONKHO_BOOK_NGOAITRU
          SET
          SOLUONG = SOLUONG + json.SOLUONGTHAYDOI
          ,NGAYCAPNHAT = getdate()
          ,NGAYHIEULUC = getdate()
          FROM OPENJSON(@json)
          WITH (
          REF_TABLE varchar(50),
