          LEFT JOIN TT_DVYEUCAU YC ON KB.KHAMBENH_ID =YC.KHAMBENH_NGOAITRU_ID
          <dynamic prepend="where">
            <isParameterPresent>
              <isNotEmpty prepend="and" property="DAXACNHAN">
                isnull(CT.DAXACNHAN,'0') = '$DAXACNHAN$'
