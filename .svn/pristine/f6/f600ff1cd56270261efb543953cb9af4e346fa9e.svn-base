    <statements>
        <!--Revision:0,c06fe90633b222278e4fa464f2991477-->
        <statement id="DAO00059_GetAll" resultClass="DataObject">
            select *
            from TM_PHONGBAN
            order by PHONGBAN_ID
        </statement>
        <!--Revision:0,8bfd8f11d464129b761b9f3f51a1e86b-->
        <statement id="DAO00059_GetList" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
            select A.* from TM_PHONGBAN A
            inner join LST_DICTIONARY B on A.LOAIPHONGBAN_ID = B.DICTIONARY_ID
            and B.DICTIONARY_TYPE_CODE = 'LoaiPhongBan' and B.DICTIONARY_CODE = 'KhoaNoi'
            where A.BENHVIEN_ID = '$BENHVIEN_ID$'
            and A.MAPHONGBAN != '$MAPHONGBAN$' AND A.TAMNGUNG = '0'
        </statement>
        <!--Revision:0,106a1da433e7fac15c8b6b0b7fa52623-->
        <statement id="DAO00059_GetDetail" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
            select *
            from TM_PHONGBAN
            where TM_PHONGBAN.PHONGBAN_ID  = '$value$'
            order by PHONGBAN_ID
        </statement>
        <!--Revision:0,380ffa9a9b6e533920c51cab2a20a8a6-->
