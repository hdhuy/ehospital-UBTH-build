        </isNotEmpty>
        <isNotEmpty prepend="and" property="PHANLOAIDUOC">
          TM_DUOC.PHANLOAIDUOC = '$PHANLOAIDUOC$'
        </isNotEmpty>
        <isNotEmpty prepend="and" property="DUONGDUNG_ID">
          TM_DUOC.DUONGDUNG_ID = '$DUONGDUNG_ID$'
