    <statement id="DAO00225_GetData" parameterClass="System.Collections.IDictionary" resultClass="DataObject">
      select a.PHANUNGTHUOCCOHAI_ID, a.SOPHIEU, a.BENHNHAN_ID, a.CANNANG, a.NGAYXUATHIENPHANUNG, a.XUATHIENPHANUNGSAU, a.MOTABIEUHIENADR
      ,a.PHANUNGNHANXET, a.NGAYLAPBAOCAO, c.TIEPNHAN_ID, d.SOBENHAN, d.BENHAN_ID
      , b.MAYTE + ' - ' + b.TENBENHNHAN as TENBENHNHAN
      from TT_NOITRU_THUPUTCH a
      join TT_BENHNHAN b on a.BENHNHAN_ID = b.BENHNHAN_ID
      join TT_TIEPNHAN c on c.BENHNHAN_ID = b.BENHNHAN_ID
      join TT_NOITRU_BENHAN d on d.TIEPNHAN_ID = c.TIEPNHAN_ID
      <dynamic prepend="where">
        <isParameterPresent>
          <isNotEmpty prepend="and" property="KEY">
            (a.SOPHIEU like '%' + #KEY# + '%' or b.MAYTE like '%' + #KEY# + '%' or b.TENBENHNHAN like '%' + #KEY# + '%')
          </isNotEmpty>
          <isNotEmpty prepend="and" property="DATEFROM">
            convert(date,a.NGAYTAO) &gt;= convert(date,'$DATEFROM$')
          </isNotEmpty>
          <isNotEmpty prepend="and" property="DATETO">
            convert(date,a.NGAYTAO) &lt;= convert(date,'$DATETO$')
          </isNotEmpty>
        </isParameterPresent>
      </dynamic>
    </statement>
