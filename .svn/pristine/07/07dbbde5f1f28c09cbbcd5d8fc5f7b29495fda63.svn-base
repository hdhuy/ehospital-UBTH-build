  <statements>
    <statement id="DAO00225_LayThongTin_ThuocNghiNgo" resultClass="DataObject" parameterClass="System.Collections.IDictionary">
      SELECT [THUOCGAYNGHINGOADR_ID]
      ,[PHANUNGTHUOCCOHAI_ID]
      ,TT.[DUOC_ID]
      ,TT.[HAMLUONG]
      ,[LIEUDUNGMOTLAN]
      ,[SOLANDUNG]
      ,TT.[DUONGDUNG_ID]
      ,[NGAYBATDAU]
      ,[NGAYKETTHUC]
      ,[HANSUDUNG]
      ,TT.[HANGSANXUAT_ID]
      ,[LYDODUNGTHUOC]
      ,D.TENDUOCDAYDU TENDUOC
      ,D.DONVITINH DVT
      ,DUONGDUNG.DICTIONARY_NAME DUONGDUNG
      FROM [TT_NOITRU_THUPUTCH_ADR] TT
      INNER JOIN TM_DUOC D ON TT.DUOC_ID=D.DUOC_ID
      LEFT JOIN LST_DICTIONARY DUONGDUNG ON TT.DUONGDUNG_ID = DUONGDUNG.DICTIONARY_ID
      WHERE PHANUNGTHUOCCOHAI_ID = '$PHANUNGTHUOCCOHAI_ID$'
    </statement>
