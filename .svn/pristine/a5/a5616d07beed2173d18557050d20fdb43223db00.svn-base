      @CACHDUNG_ID =  JSON_VALUE(@jsondatamax, '' + CHAR(36) +'.CACHDUNG_ID'),
      @KHOPHAT_ID =  JSON_VALUE(@jsondatamax, '' + CHAR(36) +'.KHOPHAT_ID'),
