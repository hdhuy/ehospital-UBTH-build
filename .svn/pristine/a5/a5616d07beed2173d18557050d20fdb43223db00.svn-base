    <statement id="DAO00014_Add_JsonKhambenhToathuoc" parameterClass="System.String" resultClass="System.Int32">
      declare @jsonVienPhi nvarchar(max);
      set @jsonVienPhi = N'{
      "DATA": $JSON_DATA$
      }';

      declare @TOATHUOC_ID int;
      declare @KHAMBENH_TOATHUOC_ID int;
