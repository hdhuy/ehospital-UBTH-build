    </statement>

    <statement id="DAO00014_Delete_1ToaThuocKhambenh" parameterClass="System.String" resultClass="System.Int32">

      DELETE FROM TT_DUOC_TONKHO_BOOK_NGOAITRU where REF_TABLE = 'TT_NGOAITRU_TOATHUOC'
